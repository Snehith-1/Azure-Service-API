<div class="card shadow-sm ">
    <div class="card-header justify-content-center  text-black bg-gradient-SD">
      <h2 class="card-title fs-3 text-black p-2"><b>Purchase Dashboard</b></h2>
    </div>
    <br>
    <div class="row m-3 ">
      <div class="col-lg-2 col-md-6 col-sm-12" (click)="GoToProduct()">
       
        <div class="card1 cardHead1 " style="border-left: 10px solid  #9a0b06 !important;margin-left: 5px;padding-top: 10px">        
          <span class="d-flex justify-content-center" style="font-size:12px ;"><i class="fa-brands fa-product-hunt" style="font-size: 22px;color:#9a1506"></i>&nbsp;&nbsp; <b style="color:#9a1c06"> Products </b> </span>
          <div >
            <p class="num" style="text-align: center ;font-size:12px ;font-weight: bold;color: black;padding-top: 4px">{{total_product}} </p>
            </div>            
      </div>                
    </div>    
        <div class="col-lg-2 col-md-6 col-sm-12" (click)="GoToVendor()">
       
          <div class="card1 cardHead1 " style="border-left: 10px solid  #069a1c !important;margin-left: 5px;padding-top: 10px">        
            <span class="d-flex justify-content-center" style="font-size:12px ;"><i class="fa-solid fa-user" style="font-size: 22px;color:#069a1c"></i>&nbsp;&nbsp; <b style="color:#069a1c"> Vendors </b> </span>
            <div >
              <p class="num" style="text-align: center ;font-size:12px ;font-weight: bold;color: black;padding-top: 4px">{{total_vendor}} </p>
              </div>            
        </div>                
      </div> 
      <div class="col-lg-2 col-md-6 col-sm-12" (click)="GoToPO()">
       
        <div class="card1  cardHead1 " style="border-left: 10px solid  #eec911 !important;margin-left: 5px;padding-top: 10px">        
          <span class="d-flex justify-content-center" style="font-size:12px ;"><i class="bi bi-hexagon-half" style="font-size: 22px;color:#eec911"></i>&nbsp;&nbsp; <b style="color:#eecd11"> PO </b> </span>
          <div >
            <p class="num" style="text-align: center ;font-size:12px ;font-weight: bold;color: black;padding-top: 4px">{{pototalcount}} </p>
            </div>            
      </div>                
    </div> 
    <div class="col-lg-2 col-md-6 col-sm-12" (click)="GoToGRN()">
       
      <div class="card1 cardHead1 " style="border-left: 10px solid  #0db1a9 !important;margin-left: 5px;padding-top: 10px">        
        <span class="d-flex justify-content-center" style="font-size:12px ;"><i class="bi bi-receipt-cutoff" style="font-size: 22px;color:#0fb99f; "></i>&nbsp;&nbsp; <b style="color:#08c5c5 ;"> GRN </b> </span>
        <div >
          <p class="num" style="text-align: center ;font-size:12px ;font-weight: bold;color: black;padding-top: 4px">{{grntotalcount}} </p>
          </div>            
    </div>                
  </div> 
  
  <div class="col-lg-2 col-md-6 col-sm-12" (click)="GoToInv()">
       
    <div class="card1  cardHead1 " style="border-left: 10px solid  #8d0486 !important;margin-left: 5px;padding-top: 10px">        
      <span class="d-flex justify-content-center" style="font-size:12px ;"><i class="fa-solid fa-money-bill-1-wave" style="font-size: 22px;color:#8d0486;"></i>&nbsp;&nbsp; <b style="color:#8d0486;"> Invoice </b> </span>
      <div >
        <p class="num" style="text-align: center ;font-size:12px ;font-weight: bold;color: black;padding-top: 4px">{{invctotalcount}} </p>
        </div>            
  </div>                
</div>     
<div class="col-lg-2 col-md-6 col-sm-12" (click)="GoToPay()">
       
  <div class="card1 cardHead1 " style="border-left: 10px solid  #05128b !important;margin-left: 5px;padding-top: 10px">        
    <span class="d-flex justify-content-center" style="font-size:12px ;"><i class="fa-solid fa-receipt" style="font-size: 22px;color:#05128b;"></i>&nbsp;&nbsp; <b style="color:#05128b;"> Payment </b> </span>
    <div >
      <p class="num" style="text-align: center ;font-size:12px ;font-weight: bold;color: black;padding-top: 4px">{{total_payment}} </p>
      </div>            
</div>                
</div> 
    </div>
  <br>


      <div class="d-flex flex-row " >
        <div class=" col-8 mb-7 cardHead radius-10  p-3 rounded">
            <div class="p-5 justify-content-center  bg-gradient-SD mb-10">
              <h3 class=" fs-3 fw-bold text-black text-center">Monthly Sales</h3>
            </div>
            
          <div id="chart" style="width: 800px; margin-left: 40px;">
          </div>
        </div>
        <div class="m-4 mt-0 ">
          <div class="card  p-3 ml-1 rounded mb-12">
            <div class="p-4 justify-content-center bg-gradient-SD">
              <h6 class="fs-3 fw-bold text-black text-center">Invoice</h6>
              <div class="card-toolbar"></div>
            </div>
            <div class="row d-flex flex-wrap m-4">
              <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-order">
                  <div class="card-body12 justify-content-center">
                    <div class="d-flex flex-wrap flex-row align-items-center">
                      <div>
                        <h6 class="mb-0 text-secondary text-center">Total</h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{ total_invoice}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle bg-gradient-Torder text-white ms-auto">
                        <i class="fa-solid fa-users"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-red1">
                  <div class="card-body12">
                    <div class="d-flex flex-wrap flex-row align-items-center">
                      <div>
                        <h6 class="mb-0 text-secondary text-center">Completed</h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{this.completed_invoice}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle bg-gradient-TQ text-white ms-auto">
                        <i class="fa-solid fa-thumbs-up"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-yellow">
                  <div class="card-body12">
                    <div class="d-flex flex-wrap flex-row align-items-center">
                      <div>
                        <h6 class="mb-0 text-secondary text-center">Pending</h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{this.pending_invoice}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle bg-gradient-CancelOrder1 text-white ms-auto">
                        <i class="fa-regular fa-hourglass-half"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-invoice">
                  <div class="card-body12">
                    <div class="d-flex flex-wrap flex-row align-items-center">
                      <div>
                        <h6 class="mb-0 text-secondary text-center">Cancelled</h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{cancelled_invoice}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle bg-gradient-Cq1 text-white ms-auto">
                        <i class="fa-solid fa-cash-register"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          
          
         <div class=" card  p-3  ">
            <div class="p-4  justify-content-center  bg-gradient-SD ">
              <h6 class=" fs-3 fw-bold text-black text-center">  Payment </h6>
              <div class="card-toolbar"></div>
            </div>
            <div class="row d-flex flex-wrap m-4 ">
  
              <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-order">
                  <div class="card-body12  justify-content-center">
                    <div class="d-flex flex-wrap flex-row  ">
                      <div>
                        <h6 class="mb-0 text-secondary text-center" ><b>Total</b> </h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{ this.total_payment }}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle bg-gradient-Torder  text-white ms-auto">
                        <i class="fa-solid fa-users"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6  col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-red1">
                  <div class="card-body12">
                    <div class="d-flex flex-wrap flex-row  ">
                      <div>
                        <h6 class="mb-0 text-secondary text-center">Completed</h6>
                        <div>
                          <h6 class="my-1 text-info text-start"> <br>{{this.completed_payment}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle  bg-gradient-TQ text-white ms-auto">
                        <i class="fa-solid fa-thumbs-up"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6  col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-yellow ">
                  <div class="card-body12">
                    <div class="d-flex flex-wrap flex-row  ">
                      <div>
                        <h6 class="mb-0 text-secondary text-center ">Pending </h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{this.pending_payment}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle  bg-gradient-CancelOrder1  text-white ms-auto">
  
                        <i class="fa-regular fa-hourglass-half"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6  col-md-6 col-sm-12">
                <div class="cardHead radius-10 border-start border-0 border-3 border-invoice ">
                  <div class="card-body12">
                    <div class="d-flex flex-wrap flex-row  ">
                      <div>
                        <h6 class="mb-0 text-secondary text-center">Cancelled</h6>
                        <div>
                          <h6 class="my-1 text-info text-start"><br>{{this.cancelled_payment}}</h6>
                        </div>
                      </div>
                      <div class="widgets-icons-2 rounded-circle  bg-gradient-Cq1 text-white ms-auto">
                        <i class="fa-solid fa-cash-register"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              
  
             
  
  
  
  
  
            </div>
          </div>
  
         
  
  
        </div>
  
      </div>
      <div class="d-flex flex-row" style="background-color: #ffffff;">

        <div class="col-12 card  shadow-lg p-3 mb-9 rounded">
            <div class="me-1">

                <div class="card-header justify-content-center bg-gradient-SD head">
                    <h4 class="card-title fs-3 fw-bold text-black">Payable Report</h4>
                </div>

                <div class="table-responsive">

                        <table id="GetMonthwiseOrderReport_List"
                            class="table table-striped table-row-bordered gy-5 gs-7 ">

                            <thead>

                                <tr
                                    class="fw-bold fs-5 text-nowrap bg-lightblue text-black align-middle  text-center">

                                    <th>S.No</th>
                                    <th class="text-start">Invoice Ref No</th>
                                    <th class="text-start">Vendor Ref No.</th>
                                    <th class="text-end">Invoice Amount</th>
                                    <th>Status</th>
                                    <th>Invoice Date</th>
                                    <th>Overall Status</th>
                                    <th>Due Date</th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr *ngFor="let data of payablesummary_list; let i=index;" class="align-middle">
                                    <td style="text-align: center;">{{i+1}}</td>
                                    <td style="text-align: start;">{{data.invoice_refno}}</td>
                                    <td style="text-align: start;">{{data.invoice_refno}}</td>
                                    <td style="text-align: end;">{{data.invoice_amount}}</td>
                                    <td style="text-align: center;">{{data.invoice_status }}</td>                                        
                                    <td style="text-align: center;">{{data.invoice_date | date: 'dd-MM-yyyy'}}</td>
                                    <td style="text-align: center;">{{data.overall_status}}</td>
                                    <td style="text-align: center;">{{data.payment_date | date: 'dd-MM-yyyy'}}</td>
                                </tr>
                            </tbody>
                        </table>
                  
                </div>

            </div>
        </div>


      </div>
    </div>
    