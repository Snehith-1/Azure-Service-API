      <div class="col-md-3 d-flex align-items-center">
        <label class="col-sm-6 col-form-label font-bold">Net Amount:</label>
        <input type="text" class="form-control fw-bold bg-secondary" formControlName="proforma_net_amount"
           style="width:120px; text-align: right;"
          readonly><span class="fw-bold ms-4 text">INR</span>
      </div>
    </div>


           
