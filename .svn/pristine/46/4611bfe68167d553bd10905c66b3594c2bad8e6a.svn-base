.primary-button {
    background-color: #42ac72;
    color: #fff !important;
    font-size: 1.1rem !important;
    font-weight: 630 !important;
    padding: 9px 16px 9px 13px !important;
    border-radius: 3px;
    border: 0;
}

.secondary-button {
    background-color: #50cd89;
    color: #fff !important;
    font-size: 1.1rem !important;
    font-weight: 630 !important;
    padding: 9px 16px 9px 13px !important;
    border-radius: 3px;
    border: 0;
}


.table-responsive {

    th {
        font-size: 11px;
        text-align: left;
        vertical-align: top;

    }

    td {
        font-size: 11px;
        text-align: left;
    }
}

.scroll { 
    padding: 5px;
    overflow: scroll;
  }
  
  ::-webkit-scrollbar {
    width: 12px;
    height: 10px;
  }
  
  ::-webkit-scrollbar-thumb {
    background: #009ef7;
  }
  
  ::-webkit-scrollbar-thumb:hover {
    background: #009ef7;
  }
  
  