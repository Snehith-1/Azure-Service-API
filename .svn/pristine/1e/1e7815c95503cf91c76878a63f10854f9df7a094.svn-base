                            <th>S.No</th>
                            <th>Team</th>
                            <th>Total</th>
                            <th>New</th>
                            <th>Pospect</th>
                            <th>Potentials</th>
                            <th>Customer</th>
                            <th>Drop</th>
                            <th>Scheduled</th>
                            <th>MTD</th>
                            <th>YTD</th>
                            <th>Lapsed Leads</th>
                            <th>Longest Leads</th>



                        </tr>
                    </thead>
                    <tbody>
                        <tr *ngFor="let data of countlist; let i=index;" class="align-middle">
                            <td>{{i+1}}</td>
                            <td>{{data.campaign_title}}</td>
                            <td>{{data.assigned_leads}}</td>
                            <td>{{data.newleads}}</td>
                            <td>{{data.prospect}}</td>
                            <td>{{data.visit}}</td>
                            <td>{{data.customer}}</td>
                            <td>{{data.drop_status}}</td>
                            <td>{{data.upcoming}}</td>
                            <td>{{data.mtd}}</td>
                            <td>{{data.ytd}}</td>
                            <td>{{data.Lapsed_Leads}}</td>
                            <td>{{data.Longest_Leads}}</td>



                        </tr>

                    </tbody>
                </table>

                <!-- Modal footer -->
                <div class="text-center my-4">
                    <!-- (click)="onclose()" -->
                    <button class="btn-danger btn-sm text-white me-4" data-bs-dismiss="modal">
                        <i class="fas fa-close text-white"></i> &nbsp;Close
                    </button>
                </div>

            </div>
        </div>
    </div>

</form>