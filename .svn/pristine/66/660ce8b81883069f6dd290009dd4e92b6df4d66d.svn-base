 <div class="m-12">
    <div class="card shadow-lg rounded-top"   style="top:-50px;">
        <div class="card m-3">
          <div class="m-3 mt-5 border card">
            <div class="card-header bg-lightblue card-height55">
              <div class="card-title m-0">
                <h5 class="fw-bolder text-blue">GRN QC Checker</h5>
              </div>
            </div>
    <form [formGroup]="reactiveForm" #form="ngForm">
        <div class="d-flex flex-row m-6 flex-wrap">
          <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Grn Ref. No</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="grn_gid"
                class="form-control" readonly />
            </div>
          </div>

          <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>GRN Date</b> <span class="ms-1 text-red">*</span>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control form-control-lg form-control-solid custom-input
              mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="grn_date" class="form-control" />
            </div>
          </div>
          <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Vendor Name</b> <span class="ms-1 text-red">*</span>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control form-control-lg form-control-solid custom-input
              mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="vendor_companyname" class="form-control" />
            </div>
          </div>
        </div>
        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Vendor Contact Person</b>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                  mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="vendor_contact_person"
                  class="form-control" readonly />
              </div>
            </div>
  
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Vendor Contact Number</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="contact_telephonenumber" class="form-control" />
              </div>
            </div>
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Email Address</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="email_id" class="form-control" />
              </div>
            </div>
          </div>
          <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Vendor Address</b>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                  mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="address"
                  class="form-control" readonly />
              </div>
            </div>
  
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Raised By</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="user_firstname" class="form-control" />
              </div>
            </div>
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>PO Ref No</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="purchaseorder_list" class="form-control" />
              </div>
            </div>
          </div>
          <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Check by User</b>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                  mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="user_checkername"
                  class="form-control" readonly />
              </div>
            </div>
  
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>DC NO</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input" formControlName="dc_no" class="form-control" />
              </div>
            </div>
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>File Upload</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                
                    <input type="file"
                        class="form-control form-control-lg form-control-solid custom-input mb-6 mb-lg-0 borderc-secondary bg-white custom-input"
                        (change)="onChange2($event)" accept=".xlsx , .xls" #fileinput class="form-control" />
                </div>
            </div>
          </div>
          <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>GRN Remarks</b>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <textarea type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                            placeholder="" readonly formControlName="grn_remarks" class="form-control"></textarea>
        
              </div>
            </div>
  
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>GRN Ref</b> <span class="ms-1 text-red">*</span>
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <textarea type="text"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                placeholder="" readonly formControlName="grn_reference" class="form-control"></textarea>

            </div>
            </div>
            
          </div>
    
                <div class="table-responsive">
                        <table id="GetGrnQcChecker_lists" class="table table-striped table-row-bordered gy-5 gs-7">
                            <thead>
                                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                                <th>Product Group</th>
                                <th>Product Code</th>
                                <th>Product</th>
                                <th>Unit</th>
                                <th>Qty Inward</th>
                                <th>Qty Shortage</th>
                                <th>Qty Deliverd</th>
                                <th>Qty Rejected</th>
                                <th>QC Remarks</th>
                                <th>Location</th>
                                <th>Bin</th>
                            </tr>
                            </thead>
                            <tbody>
                                <tr *ngFor="let data of GetGrnQcChecker_lists; let i=index;" class="fs-6 align-middle">
                               
                                <td>{{data.productgroup_name}}</td>
                                <td>{{data.product_code}}</td>
                                <td>{{data.product_name}}</td>
                                <td>{{data.productuom_name}}</td>
                                <td>{{data.qty_delivered}}</td>
                                <td style="text-align: center;">
                                   
                                        <input type="text" [(ngModel)]="data.qty_shortage" placeholder="0.00" formControlName="qty_shortage_{{i}}" class="form-control" />
                                </td>
                                  <td>
                                    <input type="text" [(ngModel)]="data.qty_delivered" placeholder="0.00" formControlName="qty_delivered_{{i}}" class="form-control" />
                                  </td>
                                  <td>
                                     <input type="text" [(ngModel)]="data.rejected_qty" placeholder="0.00"  formControlName="rejected_qty_{{i}}" class="form-control"  />
                                  </td>
                                  <td>
                                                                   
                                    <textarea formControlName="display_field_{{i}}" [(ngModel)]="data.display_field" class="form-control"></textarea>
                                  </td>
                                <td>{{data.location_name}}</td>
                                <td>{{data.bin_number}}</td>
                                </tr>
                            
                            </tbody>
                        </table>
                    </div>
                

                <div class="text-center my-4">

                    <!-- <button class=" btn-sm bg-success text-white me-2" > 
                      <i class="fas fa-check text-white fs-5"></i> &nbsp;Submit
                    </button>  -->
                    <button class="btn-sm text-white me-2" [disabled]="reactiveForm.invalid" [ngClass]="{
                        'primary-button': !reactiveForm.invalid,
                        'secondary-button': reactiveForm.invalid
                      }" (click)="validate()">
                      <i class="fas fa-check text-white fs-5"></i> &nbsp; Submit
                    </button>
                    <button class="btn-primary btn-sm text-white me-4 mt-3-center " routerLink="/pmr/PmrTrnGrncheckerSummary">
                        &nbsp;Back
                    </button>
                <!-- </div></div></div> -->

          </div>   </form> </div></div></div>
            
