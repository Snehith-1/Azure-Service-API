
<div class="card shadow-sm">
    <div class="card-header">
      <h3 class="card-title">Purchase Order Report</h3>
    </div>
 
    <div> 

    </div>
    <div class="table-responsive">
      <div class="mx-3 p-3 pt-0 pb-0 bg-white rounded table-responsive">
        <table id="PmrRptPurchaseorderdetailedreport_lists" class="table table-striped table-row-bordered gy-5 gs-7 ">
          <thead>
            <tr class="fw-bold fs-5 text-nowrap bg-lightblue align-middle text-blue">
              <th>S.NO </th>
              <th>PO Date </th>
              <th>PO Reference No</th>
              <th>Vendor Name</th>
              <th>Cost Center</th>
              <th>PO Amount</th>
              <th>GRN Status</th>
              <th>Invoice Status</th>
              <th>Payment Status</th>
              <th></th>
              
            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let data of PmrRptPurchaseorderdetailedreport_lists; let i=index;">
              <td>{{i+1}}</td>
              <td>{{data.purchaseorder_date | date:'dd-MM-yyyy'}}</td>                
              <td>{{data.purchaseorder_gid}}</td>
              <td>{{data.vendor_companyname}}</td>
              <td>{{data.costcenter_name}}</td>
              <td>{{data.total_amount}}</td>
              <td>{{data.grn_flag}}</td>
              
              <td>{{data.invoice_flag}}</td>
              <td>{{data.payment_flag}}</td>
              <td class="text-center text-nowrap"></td> 
           </tr>
          </tbody> 
        </table>
      </div>
    </div>
  </div>
  