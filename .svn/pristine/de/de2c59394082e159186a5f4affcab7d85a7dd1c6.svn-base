<div class="card m-3">
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
          <h4 class="fw-bolder text-blue">E-way Bill Entry Form</h4>
        </div>
    </div>

    <form [formGroup]="ewaybillform">

        <div class="d-flex flex-row flex-wrap bg-secondary card-height15 mt-4">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="header mt-4">
                        <div class="title">
                            <h5 class="fw-bolder ms-6 mb-3">Transaction Details</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <input type="text" formControlName="ewaybill_invoice_gid" class="form-control form-control-lg form-control-solid bg-white custom-input mb-3 mb-lg-0 border-secondary" hidden name="invoice_gid"/>

        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Supply Type<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <select class="form-select form-select-solid bg-white form-select-lg custom-input select2" formControlName="ewaybill_supply_type" [ngStyle]="{'border-color': ewaybillsupplytypeControl.invalid && ewaybillsupplytypeControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybillsupplytypeControl.invalid && (ewaybillsupplytypeControl.dirty || ewaybillsupplytypeControl.touched)">
                        <option value="" disabled selected>Select Supply Type</option>
                        <option value="Outward">Outward</option>
                        <option value="Inward">Inward</option>
                    </select>   
                    <div *ngIf="ewaybillsupplytypeControl.invalid && ewaybillsupplytypeControl.touched" class="invalid-feedback">
                        <div *ngIf="ewaybillsupplytypeControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">* Supply type is required</div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Sub Type<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <select class="form-select form-select-solid bg-white form-select-lg custom-input select2" formControlName="ewaybill_sub_type" [ngStyle]="{'border-color': ewaybillsubtypeControl.invalid && ewaybillsubtypeControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybillsubtypeControl.invalid && (ewaybillsubtypeControl.dirty || ewaybillsubtypeControl.touched)">
                        <option value="" disabled selected>Select Sub Type</option>
                        <option value="Supply">Supply</option>
                        <option value="Export">Export</option>
                        <option value="Job_Work">Job Work</option>
                        <option value="SKD/CKD/Lots">SKD/CKD/Lots</option>
                        <option value="Recipient_Not_Known">Recipient Not Known</option>
                        <option value="For_Own_Use">For Own Use</option>
                        <option value="Exhibition_or_Fairs">Exhibition or Fairs</option>
                        <option value="Line_Sales">Line Sales</option>
                        <option value="Others">Others</option>
                    </select>
                    <div *ngIf="ewaybillsubtypeControl.invalid && ewaybillsubtypeControl.touched" class="invalid-feedback">
                        <div *ngIf="ewaybillsubtypeControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">* Sub type is required</div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Document Type<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <select class="form-select form-select-solid bg-white form-select-lg custom-input select2" formControlName="ewaybill_document_type" [ngStyle]="{'border-color': ewaybilldocumenttypeControl.invalid && ewaybilldocumenttypeControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybilldocumenttypeControl.invalid && (ewaybilldocumenttypeControl.dirty || ewaybilldocumenttypeControl.touched)">
                        <option value="" disabled selected>Select Doc. Type</option>
                        <option value="Tax_Invoice">Tax Invoice</option>
                    </select>
                    <div *ngIf="ewaybilldocumenttypeControl.invalid && ewaybilldocumenttypeControl.touched" class="invalid-feedback">
                        <div *ngIf="ewaybilldocumenttypeControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">* Document type is required</div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Document Number<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input" formControlName="ewaybill_document_no" [ngStyle]="{'border-color': ewaybilldocumentnoControl.invalid && ewaybilldocumentnoControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybilldocumentnoControl.invalid && (ewaybilldocumentnoControl.dirty || ewaybilldocumentnoControl.touched)"/>
                    <div *ngIf="ewaybilldocumentnoControl.invalid && ewaybilldocumentnoControl.touched" class="invalid-feedback">
                        <div *ngIf="ewaybilldocumentnoControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">* Document number is required</div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Document Date<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="date" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input" formControlName="ewaybill_document_date" [ngStyle]="{'border-color': ewaybilldocumentdateControl.invalid && ewaybilldocumentdateControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybilldocumentdateControl.invalid && (ewaybilldocumentdateControl.dirty || ewaybilldocumentdateControl.touched)"/>
                    <div *ngIf="ewaybilldocumentdateControl.invalid && ewaybilldocumentdateControl.touched" class="invalid-feedback">
                        <div *ngIf="ewaybilldocumentdateControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">* Document date is required</div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Transaction Type<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <select class="form-select form-select-solid bg-white form-select-lg custom-input select2" formControlName="ewaybill_transaction_type" [ngStyle]="{'border-color': ewaybilltransactiontypeControl.invalid && ewaybilltransactiontypeControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybilltransactiontypeControl.invalid && (ewaybilltransactiontypeControl.dirty || ewaybilltransactiontypeControl.touched)">
                        <option value="" disabled selected>Select Trans. Type</option>
                        <option value="Regular">Regular</option>
                    </select>
                    <div *ngIf="ewaybilltransactiontypeControl.invalid && ewaybilltransactiontypeControl.touched" class="invalid-feedback">
                        <div *ngIf="ewaybilltransactiontypeControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">* Transaction type is required</div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="d-flex flex-row flex-wrap bg-secondary card-height15">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <div class="header mt-4">
                                    <div class="title">
                                        <h5 class="fw-bolder ms-3 mb-3">Bill From</h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Name</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_bill_fromname" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>GSTIN</b>
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_bill_fromgstin" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>State</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_bill_fromstate" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>                    
                </div>
            </div>

            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="d-flex flex-row flex-wrap bg-secondary card-height15">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <div class="header mt-4">
                                    <div class="title">
                                        <h5 class="fw-bolder ms-3 mb-3">Dispatch From</h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Address</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_dispatch_fromaddress" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>
                    
                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Place</b>
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_dispatch_fromplace" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>

                        <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Pincode</b>
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_dispatch_frompincode" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                        
                        <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>State</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_dispatch_fromstate" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="d-flex flex-row flex-wrap bg-secondary card-height15">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <div class="header mt-4">
                                    <div class="title">
                                        <h5 class="fw-bolder ms-3 mb-3">Bill To</h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Name</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_bill_toname" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>GSTIN</b>
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_bill_togstin" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>State</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_bill_tostate" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>                    
                </div>
            </div>

            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="d-flex flex-row flex-wrap bg-secondary card-height15">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <div class="header mt-4">
                                    <div class="title">
                                        <h5 class="fw-bolder ms-3 mb-3">Ship To</h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Address</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_ship_toaddress" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>
                    
                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Place</b>
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_ship_toplace" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                        <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Pincode</b>
                            <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_ship_topincode" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                        <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>State</b>
                            <div class="mt-2">
                                <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_ship_tostate" style="color: rgb(163, 17, 17);" readonly/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row flex-wrap bg-secondary card-height15 mt-4">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="header mt-4">
                        <div class="title">
                            <h5 class="fw-bolder ms-6 mb-3">Invoice Details</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row flex-wrap m-6">
            <div class="col-12 col-lg-7 col-md-7 col-sm-12"><b>IRN</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_irn" style="color: rgb(163, 17, 17);" readonly/>
                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Invoice Reference No</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_invoice_ref_no" style="color: rgb(163, 17, 17);" readonly/>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Invoice Amount</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_invoice_amount" style="color: rgb(163, 17, 17);" readonly/>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row flex-wrap bg-secondary card-height15 mt-4">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="header mt-4">
                        <div class="title">
                            <h5 class="fw-bolder ms-6 mb-3">Items Details</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="table-responsive">
            <div class="card card-body p-3 mb-5 bg-white rounded">
              <table id="product" class="table table-striped table-row-bordered gy-4 gs-2">
                <thead class="table_head">
                  <tr class="fw-bold fs-6 text-nowrap">
                    <th style="text-align: center;">S.No</th>
                    <th>Product Name</th>                    
                    <th>Product Description</th>
                    <th>HSN Code / Description</th>
                    <th>Unit</th>
                    <th style="text-align: center;">Quantity</th>
                    <th style="text-align: right;">Discount (%)</th>
                    <th style="text-align: right; width: 120px;">Tax</th>
                    <th style="text-align: right;">Total Amount</th>
                  </tr>
                </thead>
                <tbody>
                  <tr > <!-- *ngFor="let data of invoiceproductlist; let i=index;" -->
                    <td style="text-align: center;"></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td style="text-align: center;"></td>
                    <td style="text-align: right;"></td>
                    <td style="text-align: right;"></td>
                    <td style="text-align: right; font-weight: bold;"></td>
                  </tr>
                </tbody>
              </table>
            </div>
        </div>

        <div class="d-flex flex-row flex-wrap bg-secondary card-height15 mt-4">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="header mt-4">
                        <div class="title">
                            <h5 class="fw-bolder ms-6 mb-3">Transportation Details</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Transporter ID</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_transporter_id"/>
                </div>
            </div>

            <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Transporter Name</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_transporter_name"/>
                </div>
            </div>

            <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Approximate Distance (in KM)<span class="text-red ms-2">*</span></b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2" style="display: flex; align-items: center;">
                    <div style="display: flex; flex-direction: column;">
                        <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input" formControlName="ewaybill_approximate_distance" [ngStyle]="{'border-color': ewaybillapproximatedistanceControl.invalid && ewaybillapproximatedistanceControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="ewaybillapproximatedistanceControl.invalid && (ewaybillapproximatedistanceControl.dirty || ewaybillapproximatedistanceControl.touched)" style="width:100px; color: rgb(163, 17, 17);"/>
                        <span style="margin-left: 110px; margin-top: -26px; font-weight: bold;">Auto Calculated PIN to PIN (in KM)</span>
                        <div *ngIf="ewaybillapproximatedistanceControl.invalid && ewaybillapproximatedistanceControl.touched" class="invalid-feedback">
                            <div *ngIf="ewaybillapproximatedistanceControl.hasError('required')" style="color: red; width: 200px; margin-top: 8px; margin-left: 8px; font-size: 11px;">* Distance is required</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row flex-wrap bg-secondary card-height15 mt-4">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row mt-2">
                    <div class="header mt-4">
                        <div class="title">
                            <h5 class="fw-bolder ms-6 mb-3">Part B</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Transport Mode</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <select class="form-select form-select-solid bg-white border-secondary form-select-lg custom-input select2" formControlName="ewaybill_transport_mode">
                        <option value="" disabled selected>Select Mode</option>                        
                        <option value="1">Road</option>
                        <option value="2">Rail</option>
                        <option value="3">Air</option>
                        <option value="4">Ship or Ship Cum Road/Rail</option>
                    </select>
                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vehicle Type</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <select class="form-select form-select-solid bg-white border-secondary form-select-lg custom-input select2" formControlName="ewaybill_vehicle_type">
                        <option value="" disabled selected>Select Type</option>
                        <option value="R">Regular</option>
                        <option value="O">Over Dimensional Cargo</option>
                    </select>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Vehicle Number</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_vehicle_no"/>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Transporter Document Number</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <input type="text" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_transporter_doc_no"/>
                </div>
            </div>

            <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Transporter Date</b>
                <div class="col-12 col-lg-11 col-md-11 col-sm-11 fv-row d-flex mt-2">
                    <input type="date" class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input" formControlName="ewaybill_transporter_date"/>
                </div>
            </div>
        </div>

        <div class="text-center my-4">
            <button type="submit" class="btn-success btn-sm text-white me-2" (click)="ewaybillSubmit()" [disabled]="ewaybillform.invalid" [ngClass]="{'primary-button': !ewaybillform.invalid,'secondary-button': ewaybillform.invalid}">
                <i class="bi bi-check-square fs-5"></i>
                <span class="ms-3">Submit</span>
            </button>
        
            <button type="back" class="btn-primary btn-sm text-white me-4" (click)="back()">
                <i class="bi bi-arrow-left-square fs-5 me-1"></i>
                <span class="ms-3">Back</span>
            </button>
        </div>
    </form>
</div>