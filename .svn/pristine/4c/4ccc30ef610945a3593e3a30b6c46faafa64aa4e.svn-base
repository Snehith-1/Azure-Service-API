<div class="card m-3">
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">View GRN </h5>
        </div>
    </div>

    <div *ngFor="let data of GetEditGrnInward_lists;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Ref No</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.grn_gid}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Date</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_companyname}}</p>
                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact Number</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_contact_person}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Address</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.address}}</p>
                </div>
            </div>
        </div>
    </div>
    <div *ngFor="let data of GetEditGrnInward_lists;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Ref. No</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.purchaseorder_list}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Raised By</b>

                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.user_firstname}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Remarks</b>

                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.grn_remarks}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Ref</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.grn_reference}}</p>
                </div>
            </div>
        </div>
    </div>
    <div *ngFor="let data of GetEditGrnInward_lists;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>DC NO</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.dc_no}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Invoice Ref.No </b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.invoice_refno}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Name</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_companyname}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact person</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_contact_person}}</p>
                </div>
            </div>
        </div>
    </div>
    <div *ngFor="let data of GetEditGrnInward_lists;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Email Address</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.email_id}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Check by User</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.user_checkername}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Approved By</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.user_approvedby}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Approver Remarks</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.reject_reason}}</p>
                </div>
            </div>
        </div>
    </div>
    <div *ngFor="let data of GetEditGrnInward_lists;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>DC Date</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.dc_date}}</p>
                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Invoice Date</b>
                <div class="col-lg-11 col-12 col-md-12 col-sm-12 fv-row mt-2">
                    <p>{{data.invoice_date}}</p>
                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Priority</b>
                <div class="d-flex justify-content-start mt-4 ms-2">
                    
                    <p>{{data.priority_n}}</p>
                </div>
            </div>
        </div>
    </div>
                
        <div class="table-responsive">
            <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
                <table id="GetEditGrnInwardproduct_lists" class="table table-striped table-row-bordered gy-5 gs-7">
                    <thead>
                        <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        
                        <th>Product Group</th>
                        <th>Product Code</th>
                        <th>Product</th>
                        <th>Unit</th>
                        <th>Qty Ordered</th>
                        <th>Qty Received</th>
                        <th>Qty Adjusted</th>
                        <th>Qty Rejected</th>
                        <th>Remarks</th>
                        <th>QC Remarks</th>
                        <th>Location</th>
                        <th>Bin</th>
                    </tr>
                    </thead>
                    <tbody>
                        <tr *ngFor="let data of GetEditGrnInwardproduct_lists;"> 
                        
                        <td>{{data.productgroup_name}}</td>
                        <td>{{data.product_code}}</td>
                        <td>{{data.product_name}}</td>
                        <td>{{data.productuom_gid}}</td>
                        <td>{{data.qty_ordered}}</td>
                        <td>{{data.qty_received}}</td>
                        <td>{{data.qty_grnadjusted}}</td>
                        <td>{{data.qty_rejected}}</td>
                        <td>{{data.product_remarks}}</td>
                        <td>{{data.location_name}}</td>
                        <td>{{data.bin_number}}</td>
                        <td>{{data.qc_remarks}}</td>
                        </tr>
                    
                    </tbody>
                </table>
            </div>
        </div>

        <div class="text-center my-4">
            <button class="btn-primary btn-sm text-white me-4 mt-3-center " routerLink="/pmr/PmrTrnGrninward">
                &nbsp;Back
            </button>
        </div>

    