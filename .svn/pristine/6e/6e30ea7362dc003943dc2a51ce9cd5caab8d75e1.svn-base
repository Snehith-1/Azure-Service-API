<div class="card shadow-sm">
  <div class="card-header bg-lightblue card-height55">
    <div class="card-title m-0">
      <h3 class="fw-bolder text-blue">RAISE SALES ORDER</h3>
    </div>
  </div><br>
  <form [formGroup]="OrderForm" (ngSubmit)="onSubmit()">

    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Ref. No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">

          <input formControlName="customer_gid" hidden
          placeholder="Address Line 1"  type="text" class="
              form-control form-control-lg form-control-solid
              bg-white
              custom-input
              mb-3 mb-lg-0
              borderc-secondary
            " name="customer_gid" />


          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="New Purchase Order" formControlName="quotation_gid" class="form-control" />
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Date</b><span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">

          <input type="date"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input "
            formControlName="salesorder_date" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch Name<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">

          <input
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            readonly formControlName="branch_name" placeholder="Auto Populate" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>QO Ref. No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="New Purchase Order" readonly formControlName="quotation_referenceno1" class="form-control" />
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Customer Name<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            formControlName="customer_name" readonly placeholder="Auto Populate">
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Ref. No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="New Purchase Order" formControlName="so_referencenumber" class="form-control" />
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Person<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" readonly formControlName="customercontact_names" class="form-control" />
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Number</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" formControlName="customer_mobile" class="form-control" />
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Email Address</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" formControlName="customer_email" class="form-control" />
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Address</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" formControlName="customer_address" class="form-control">
  </textarea>
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Remarks</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" readonly formControlName="so_remarks" class="form-control">
</textarea>
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping Address</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Shipping Address" readonly formControlName="shipping_to" class="form-control">
  </textarea>
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Sales Person<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select [items]="user_list" class="
  custom
  col-lg-12 col-12
  fv-row
  mt-2" bindValue="user_gid" bindLabel="user_name" formControlName="user_name" placeholder="Select an option">
          </ng-select>
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Expected Start Date</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="date"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input "
            formControlName="start_date"/>
        </div>
      </div>


      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Esimated Time of Arrival</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="date"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input "
            formControlName="end_date" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Freight terms" formControlName="freight_terms" class="form-control" />
        </div>
      </div>
    </div>


    <div class="d-flex flex-row m-6 flex-wrap">

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Payment Terms" formControlName="payment_terms" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select [items]="currency_list" class="
    custom
    col-lg-12 col-12
    fv-row
    mt-2 " bindValue="currencyexchange_gid" bindLabel="currency_code" formControlName="currency_code"
            placeholder="Select an option" (change)="OnChangeCurrency($event)"
            (change)="onCurrencyCodeChange($event)"></ng-select>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchange Rate</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Exchange Rate" formControlName="exchange_rate" class="form-control" />
        </div>
      </div>
    </div>

    <!-- end:: PO Add Form -->

    <!-- begin::Product Summary -->


    <!-- begin::Heading -->
    <div class="card-header bg-lightblue card-height55" style="border-radius: 0px;">
      <div class="card-title m-0">
        <h5 class="fw-bolder text-blue">PRODUCT SUMMARY</h5>
      </div>
    </div>

    <form [formGroup]="productform" (ngSubmit)="OnSubmit()">
      <div class="d-flex flex-row flex-wrap m-6">
        <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Product<span class="ms-1 text-red">*</span></b>
            <ng-select [items]="product_list" class="custom col-lg-11 col-12 fv-row mt-2" bindLabel="product_name" (change)="GetOnChangeProductName()" formControlName="product_name" placeholder="Select an option" ></ng-select>
          <!-- </div> -->
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Product Code</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text" class="form-control" formControlName="product_code" readonly style="background-color: #e9e7e7;">
          </div>
        </div>


        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Unit</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text" class="form-control" style="text-align: right;" formControlName="productuom_name">
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Unit Price</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text" class="form-control" placeholder="0.00" style="text-align: right;" formControlName="selling_price" [(ngModel)]="unitprice" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Quantity</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text" class="form-control" style="text-align: right;" formControlName="quantity" [(ngModel)]="quantity" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Discount(%)</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text" class="form-control mt-2" placeholder="0.00"  style="background-color: #e9e7e7; text-align: right;" formControlName="discountpercentage" [(ngModel)]="discountpercentage" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
            <input type="text" class="form-control" placeholder="0.00" style="text-align: right;" formControlName="discountamount" [(ngModel)]="discountamount" (input)="prodtotalcal()">            
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Tax<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <!-- <ng-select [items]="tax_list" bindLabel="tax_name" formControlName="tax_name" bindValue="tax_gid" placeholder="Select Tax" (change)="taxAmount()"></ng-select> -->
            <ng-select [items]="tax_list" class="
                      custom
              col-lg-10 col-12
              fv-row
              mt-2
            " bindLabel="tax_name" formControlName="tax_name" bindValue="tax_gid" (change)="OnChangeTaxAmount1()"
                  placeholder="Select Tax" [(ngModel)]="mdlTaxName1"></ng-select>
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
             <input type="text" class="form-control" placeholder="0.00" readonly
                  style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount"
                  [(ngModel)]="tax_amount" (input)="OnChangeTaxAmount1()">       </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Total Amount</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text" class="form-control" placeholder="0.00" style="background-color: #e9e7e7; text-align: right;" formControlName="totalamount" [(ngModel)]="totalamount" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12">
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
            <button type="submit" class="btn-primary"(click)="OnSubmit()">+</button>
          </div>
        </div>
      </div>
      

     

      
    </form>

    <!-- end::Product Form -->
    <!-- begin::table -->

    <div class="table-responsive">
      <div class="card card-body table-responsive m-0 shadow-lg p-3 mb-5 bg-white rounded">
        <table id="QSOproductlist" class="table table-striped table-row-bordered gy-4 gs-2">
          <thead class="table_head">
            <tr class="text-white fw-bold fs-5 text-nowrap bg-lightblue text-blue">
              <th>S.No</th>
              <th>Product Name</th>
              <!-- <th>Product Group</th> -->
              
              <th>Product Code</th>
              <!-- <th>HSN Code / Description</th> -->
              <th>Unit</th>
              <th>Unit Price</th>
              
              <th>Quantity</th>
              <th>Discount (%)</th>
              
              <th>Tax</th>
              <th>Total Amount</th>
              <th class="text-center">Action</th>
            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let data of QSOproductlist; let i=index;">
              <td>{{i+1}}</td>
              <td>{{data.product_name}}</td>
              <!-- <td>{{data.productgroup_name}}</td> -->
              
              <td>{{data.product_code}}</td>
              <td>{{data.productuom_name}}</td>
              <td>{{data.product_price}}</td>
              
              <td>{{data.quantity}}</td>
              <td>{{data.discountpercentage}}<br>{{data.discountamount}}</td>
              
              <td>
                <div class="container" >
                  <div class="row">                   
                      {{data.tax_name}}
                    </div>
                    <div class="row">
                    {{data.tax_amount}}
                  </div>
                  
                  <!-- <div class="row">
                    <div class="col">
                      {{data.tax_name2}}

                    </div>
                    {{data.tax_amount2}}

                  </div> -->
                  <!-- <div class="row">
                    <div class="col">
                      {{data.tax_name3}}

                    </div>
                    {{data.tax_amount3}}

                  </div> -->
                </div>
              </td>
              <td>{{data.price}}</td>
              <td class="text-center text-nowrap">
                <!-- <button title="edit" type="button" (click)="onedit(data.product_gid)" data-bs-toggle="modal"
                  data-bs-target="#myModaldelete" class="btn btn-icon btn-sm bg-green me-2"><span
                    class="fa-solid fa-pencil text-white  fs-6"></span></button> -->
                <button title="Delete" type="button" (click)="ondelete(data.product_gid)" data-bs-toggle="modal"
                  data-bs-target="#myModaldelete" class="btn btn-icon btn-sm bg-danger me-2"><span
                    class="fa-solid fa-trash text-white  fs-6"></span></button>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
    <!-- end::table -->
    <!-- begin::calculation part -->
    <div class="card">
     
        <div class="d-flex flex-row m-6 flex-wrap">
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Payment Days</b><span class="ms-1 text-red">*</span>
            <div class="col-lg-8 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="number"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
                formControlName="payment_days" class="form-control" placeholder="Payment Days" required/>

            </div>

            <div *ngIf="payment_days.invalid && (payment_days.dirty || payment_days.touched)">
              <div *ngIf="payment_days.errors?.['required']">
                <span style="color:red;">Enter Payment Days.</span>
              </div>
            </div>
          </div>
          

          <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Delivery Days</b> <span class="ms-1 text-red">*</span>
            <div class="col-lg-8 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="number" placeholder="Delivery Days"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
                formControlName="delivery_days" class="form-control" required/>
            </div>
            <div *ngIf="delivery_days.invalid && (delivery_days.dirty || delivery_days.touched)">
              <div *ngIf="delivery_days.errors?.['required']">
                <span style="color:red;">Enter Delivery Days.</span>
              </div>
            </div>
          </div>
        </div>
    
      <div class="card">
        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Total:</label>
            <input type="text" class="form-control" style="width:70px" style="background-color: #e9e7e7; " readonly formControlName="total_price" [(ngModel)]="total_price"
            (input)="finaltotal()" placeholder="0.00">
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Tax:</label>


            <ng-select class=" custom
                    col-lg-10 col-12
                    fv-row
                    mt-2" style="width:70px" [items]="tax4_list" formControlName="tax_name4" bindLabel="tax_name4"
              bindValue="tax_gid" placeholder="Tax" (change)="OnChangeTaxAmount4()">
            </ng-select>&nbsp;
            <input type="text" class="form-control" style="width:70px" (input)="OnChangeTaxAmount4()" readonly
            style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount4" (input)="prodtotalcal()"
            [(ngModel)]="tax_amount4">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>


        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Total With Tax:</label>
            <input type="text" class="form-control" formControlName="total_amount" style="width:70px"
              placeholder="0.00" [(ngModel)]="total_amount" (input)="finaltotal()">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Addon Charges:</label>
            <input type="text" class="form-control" formControlName="addon_charge" style="width:70px"
              placeholder="0.00" [(ngModel)]="addon_charge" (input)="finaltotal()">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Additional Discount:</label>
            <input type="text" class="form-control"  formControlName="additional_discount" style="width:70px"
              placeholder="0.00 " [(ngModel)]="additional_discount" (input)="finaltotal()">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Freight Charges:</label>
            <input type="text" class="form-control" formControlName="frieght_charges"  style="width:70px"
              placeholder="0.00" [(ngModel)]="freight_charges" (input)="finaltotal()">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>
        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Buy Back/Scrap:</label>
            <input type="text" class="form-control" formControlName="buyback_charges"  style="width:70px"
              placeholder="0.00" (input)="finaltotal()" [(ngModel)]="buyback_charges">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Packing/ Forwarding Charges :</label>
            <input type="text" class="form-control"  formControlName="packing_charges" style="width:70px"
              placeholder="0.00" (input)="finaltotal()" [(ngModel)]="packing_charges">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Insurance Charges:</label>
            <input type="text" class="form-control"  formControlName="insurance_charges" style="width:70px"
              placeholder="0.00" (input)="finaltotal()" [(ngModel)]="insurance_charges">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Round Off:</label>
            <input type="text" class="form-control" formControlName="roundoff"  style="width:70px"
              placeholder="0.00" (input)="finaltotal()" [(ngModel)]="roundoff">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Grand Total:</label>
            <input type="text" class="form-control" readonly formControlName="Grandtotal" (input)="finaltotal()" [(ngModel)]="Grandtotal" style="width:120px"
              placeholder="0.00">&nbsp;
            <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
          </div>
        </div>




        <!--Summary count ends-->

        <div class="text-center my-4">
          <button class="btn-success btn-sm text-white me-4" data-bs-dismiss="modal">
            <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
          </button> &nbsp;&nbsp;
          <button class="btn-danger btn-sm text-white me-4" (click)="close()" data-bs-dismiss="modal">
            <i class="fas fa-close text-white"></i> &nbsp;Close
          </button>
        </div>
      </div>
    </div>
  </form>
</div>