<!-- begin::Add PO -->
<div class="card m-3">
  <!-- begin::Heading -->
  <div class="card-header bg-primary card-height55">
    <div class="card-title m-0">
      <h3 class="fw-bolder text-white"> <i class="fas fa-plus fw-bolder fs-3"></i>&nbsp;&nbsp;Direct Purchase Order</h3>
    </div>
  </div>
  <!-- end::Heading -->

  <!-- begin:: PO Add Form-->
  <form [formGroup]="POAddForm" #form="ngForm" (ngSubmit)="onSubmit()">
    <div  class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">PR Ref. No</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input "
            placeholder="PO Ref. No" style="height:40px;"  class="form-control" readonly />
        </div>
      </div>

<!-- 
      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">PO Date</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="date"
            class="form-control form-control-lg form-control-solid date-picker mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="DD-MM-YYYY" style="height: 40px;;" />
        </div>
      </div> -->
      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">PO Date<span class="ms-1 text-red">*</span></span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="date"   pattern="^(?!\s*$).+" [ngStyle]="{ 'border-color': POAddForm.get('po_date')?.hasError('required')
             && POAddForm.get('po_date')?.touched ? 'red' : 'rgb(209, 209, 209)'}"
            formControlName="po_date"
       class="form-control form-control-lg form-control-solid date-picker custom-input mb-3 mb-lg-0 bg-white
       custom-input date-picker "  placeholder="DD-MM-YYYY" style="height: 40px;"/>
    </div>
</div> 






      
      <div class="col-12 col-lg-4 col-md-4 col-sm-12">
        <span class="font-bold">Branch Name<span class="ms-1 text-red">*</span></span>
        <div class="col-lg-11 col-15 col-md-13 col-sm-13 fv-row mt-2">
          <ng-select formControlName="branch_name" (change)="OnChangeBranch()"
            [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
            [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="branch_list" required
            class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="branch_name"
            bindValue="branch_gid" placeholder="Select an option" [(ngModel)]="mdlBranchName">
          </ng-select>
          <div *ngIf="
                            product_code.invalid &&
                                (product_code.dirty || product_code.touched)
                              " class="invalid-feedback">
            <div *ngIf="product_code.errors?.['required']">
              Product Code is required.
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">

      <div class="col-12 col-lg-4 col-md-3 col-sm-12"> <span class="font-bold">PO No</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white "
            placeholder="New Purchase Order"  formControlName="po_no" class="form-control" style="height:40px ;" />
        </div>
      
      </div>
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Despatch To Branch<span class="ms-1 text-red">*</span></span>
        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select formControlName="dispatch_name"
            [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
            [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="dispatch_list"
            required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="dispatch_name"
            bindValue="branch_gid" placeholder="Select an option" [(ngModel)]="mdlDispatchName">
            <option *ngFor="let dispatch of dispatch_list" [value]="dispatch.branch_gid">
              {{dispatch.dispatch_name}}</option>
          </ng-select>
          <div *ngIf="
                            product_code.invalid &&
                                (product_code.dirty || product_code.touched)
                              " class="invalid-feedback">
            <div *ngIf="product_code.errors?.['required']">
              Despatch Branch Name is required.
            </div>
          </div>
        </div>
      </div>
    
      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold"> Priority</span>
       
        <div class="d-flex justify-content-start mt-4 ms-2">
          <div class="form-check form-check-custom form-check-primary form-check-solid form-check-sm">
            <input class="form-check-input" type="radio" name="priority" formControlName="priority_n" value="Y"
              (change)="showTextBox($event)" />
            <label class="form-check-label text-gray-700 text-nowrap" for="priorityYes">Yes</label>
          </div>
          <div class="form-check form-check-custom form-check-primary form-check-solid form-check-sm ms-10">
            <input class="form-check-input" type="radio" name="priority" formControlName="priority_n" value="N"
              (change)="showTextBox($event)" />
            <label class="form-check-label text-gray-700 text-nowrap" for="priorityNo">No</label>
          </div>
        </div>
        <br>
        <div *ngIf="showInput">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            formControlName="priority_remarks" class="form-control" [(ngModel)]="inputValue"
            placeholder="Enter text here"></textarea>
        </div>
      </div>
    
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Vendor Name<span class="ms-1 text-red">*</span></span>
        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select formControlName="vendor_companyname" (change)="OnChangeVendor()"
            [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
            [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="vendor_list" required
            class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="vendor_companyname"
            bindValue="vendor_gid" placeholder="Select an option" [(ngModel)]="mdlVendorName">
            <option *ngFor="let vendor of vendor_list" [value]="vendor.vendor_gid">
              {{vendor.vendor_companyname}}</option>
          </ng-select>
          <div *ngIf="
                            product_code.invalid &&
                                (product_code.dirty || product_code.touched)
                              " class="invalid-feedback">
            <div *ngIf="product_code.errors?.['required']">
              Vendor Name is required.
            </div>
          </div>
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Vendor Contact Person</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary custom-input"
            placeholder="Auto Populate" style="height: 40px;" readonly formControlName="contact_person" class="form-control bg-secondary " />
        </div>
      </div>
   
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Vendor Mobile</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary  custom-input"
            placeholder="Auto Populate" style="height:40px;" readonly formControlName="contact_number" class="form-control bg-secondary" />
        </div>
      </div>
    
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Vendor Email </span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" style="height: 40px;" formControlName="email_address" class="form-control bg-secondary" />
        </div>
        
      </div>
      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">Vendor Address</span>
        <div class="col-lg-11 col-12 col-md-12 col-sm-11 fv-row mt-2">
            <textarea type="text"  class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary  custom-input"
             formControlName="vendor_address"
                class="form-control bg-secondary"></textarea>
        </div>
    </div>

      <!-- <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Vendor Address</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" style="height: 40px;" readonly formControlName="vendor_address" class="form-control bg-secondary"></textarea>
        </div>
      </div> -->

      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Vendor Fax</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Vendor Fax" style="height: 40px;" formControlName="vendor_fax" class="form-control" />
        </div>
      </div>

    </div>
<div  class="d-flex flex-row m-6 flex-wrap">
  <div class="col-12 col-lg-4 col-md-3 col-sm-12">
    <span class="font-bold">Ship Via</span>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="text"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
        placeholder="Ship Via" style="height: 40px;" formControlName="ship_via" class="form-control" />
    </div>
  </div>
  <div class="col-12 col-lg-4 col-md-3 col-sm-12">
    <span class="font-bold">Shipping Address</span>
    <div class="col-lg-11 col-12 col-md-12 col-sm-12 fv-row mt-2">
      <textarea type="text" rows="1"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
        placeholder="Shipping Address" style="height:40px;" formControlName="Shipping_address" class="form-control"
        ></textarea>
    </div>
  </div>
  <div class="col-12 col-lg-4 col-md-3 col-sm-12">
    <span class="font-bold">Delivery Location </span>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="text"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
        placeholder="Delivery Location" style="height: 40px;" formControlName="delivery_location" class="form-control" />
    </div>
  </div>

</div>

    <div class="d-flex flex-row m-6 flex-wrap">
     
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Freight Terms</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Freight Terms" style="height: 40px;" formControlName="freight_terms" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Payment Terms</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Payment Terms" style="height: 40px;" formControlName="payment_terms" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">PO Cover Note</span> 
        <div class="col-lg-11 col-12 col-md-12 col-sm-11 fv-row mt-2">
            <textarea type="text"  class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary  bg-white custom-input"
             formControlName="pocovernote_address"
                class="form-control "></textarea>
        </div>
    </div>
     
      <!-- <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">PO Cover Note</span> 
        <div class="col-lg-11 col-12 col-md-12 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Auto Populate"  style="height: 40px;" formControlName="pocovernote_address" class="form-control">Sub: Purchase Order.
                        Ref: As per your Quotation.
                        Please print the above said items with the following descriptions.</textarea>
        </div>
      </div> -->
      
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold">Currency<span class="ms-1 text-red">*</span></span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <!-- <select class="form-select form-select-solid bg-white form-select-lg borderc-secondary custom-input select2"
            formControlName="currency_code" (change)="OnChangeCurrency($event)" (change)="onCurrencyCodeChange($event)">
            <option value="" disabled selected>Select Currency</option>
            <option *ngFor="let currency of currency_list" [value]="currency.currencyexchange_gid">
              {{currency.currency_code}}</option>
          </select> -->
          <ng-select formControlName="currency_code"
          [items]="currency_list" (change)="OnChangeCurrency()"
          required class="custom col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="currency_code"
          bindValue="currencyexchange_gid" placeholder="Select an option" [(ngModel)]="mdlCurrencyName">
          <option *ngFor="let currency of currency_list" [value]="currency.currencyexchange_gid" style="height: 40px;">
            {{currency.currency_code}}</option>
        </ng-select>
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-3 col-sm-12">
        <span class="font-bold"> Exchange Rate</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2 ">
          <input type="number" style="text-align:right;"
          class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input "
            placeholder="0.00"formControlName="exchange_rate" style="height: 40px;" class="form-control" />
        </div>
      </div>

      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">Remarks</span>
        <div class="col-lg-11 col-12 col-md-12 col-sm-11 fv-row mt-2">
            <textarea type="text"  class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary  bg-white custom-input"
             formControlName="remarks"
                class="form-control "></textarea>
        </div>
    </div>

      <!-- <div class="col-12 col-lg-4 col-md-12 col-sm-12">
        <span class="font-bold">Remarks</span>
        <div class="col-12 col-lg-11 col-md-12 col-sm-12 fv-row mt-2">
          <textarea type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="" formControlName="remarks" style="height: 40px;" class="form-control"></textarea>
        </div>
      </div> -->
    
    </div>

    <!-- end:: PO Add Form -->

    <!-- begin::Product Summary -->
    <div class="card shadow-sm">
      <!-- begin::Heading -->
      <div class="card-header bg-lightblue card-height55" style="border-radius: 0px;">
        <div class="card-title m-0">
          <h5 class="fw-bolder text-blue">Product Summary</h5>
        </div>
      </div>
      <!-- end::Heading -->
      <!-- begin::Product Form -->
      <form [formGroup]="productform" (ngSubmit)="productSubmit()">
        <div class="d-flex flex-row m-5 flex-wrap" >
          <div class="col-12 col-lg-2 col-md-2 col-sm-12">
           <span class="font-bold"> Product Name<span class="ms-1 text-red">*</span></span>
        
              <ng-select formControlName="productname" class="custom col-lg-11 col-12 fv-row mt-2" (change)="GetOnChangeProductName()" [class.is-invalid]="
                        product_name.invalid &&
                            (product_name.dirty || product_name.touched)
                          " [ngClass]="{'invalid-border': product_name.invalid && product_name.touched}"
                [items]="productdetails_list" required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                bindLabel="product_name"  placeholder="Select an option" [(ngModel)]="mdlProductName">
              </ng-select>
              <div *ngIf="
                        product_name.invalid &&
                            (product_name.dirty || product_name.touched)
                          " class="invalid-feedback">
                <div *ngIf="product_name.errors?.['required']">
                  Product Name is required.
                </div>
              </div>
          
          
          </div>
       
          <div class="col-12 col-lg-1 col-md-1 col-sm-12">
            <span class="font-bold">Product Code</span>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control bg-secondary" style="height:40px;" formControlName="productcode" readonly >
            </div>
        </div>
        <div class="col-12 col-lg-1 col-md-1 col-sm-12">
          <span class="font-bold">Unit</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control bg-secondary" style="height:40px;" formControlName="productuom_name" readonly >
            </div>
        </div>
        

       
        
        <div class="col-12 col-lg-1 col-md-1 col-sm-12">
          <span class="font-bold">Unit Price</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="number" class="form-control" placeholder="0.00" style="text-align: right;"
              formControlName="unitprice" [(ngModel)]="unitprice" style="height:40px;" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-12 col-lg-1 col-md-1 col-sm-12">
          <span class="font-bold">Quantity</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="number" class="form-control" style="text-align: right; height:40px;" formControlName="quantity"
              [(ngModel)]="quantity" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-lg-1 col-md-1 col-sm-1">
          <span class="font-bold">Discount(%)</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="number" class="form-control" placeholder="0.00" style="text-align: right;height:40px;"
              formControlName="discountpercentage" [(ngModel)]="discountpercentage" (input)="prodtotalcal()">
          </div>

          <!-- <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="number" class="form-control" placeholder="0.00" readonly
              style="background-color: #e9e7e7; text-align: right;" formControlName="discountamount"
              [value]="discountamount" [(ngModel)]="discountamount" (input)="prodtotalcal()">
          </div> -->
        </div>

        <div class="col-lg-1 col-md-1 col-sm-1">
          <span class="font-bold">Discount Amount</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="text" class="form-control bg-secondary" placeholder="0.00"
              style="text-align: right;height:40px;" formControlName="discountamount" 
              [(ngModel)]="discountamount"  (input)="prodtotalcal()" readonly>
          </div>
        </div>



        <div class="col-lg-1 col-md-1 col-sm-1">
          <span class="font-bold">Tax</span> 
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
              <ng-select formControlName="tax_name1" (change)="OnChangeTaxAmount1()"
                      [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
                      [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="tax_list"
                      required class="custom col-lg-12 col-12 col-md-12 col-sm-12 fv-row mt-2" bindLabel="tax_name1"
                      bindValue="tax_gid" placeholder="Select an option" [(ngModel)]="mdlTaxName1">
              </ng-select>
          </div>
         
            <!-- <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
              <input type="text" class="form-control" placeholder="0.00" readonly
                style="text-align: right; background-color: #e9e7e7;" formControlName="taxamount1"
                [(ngModel)]="taxamount1" (input)="OnChangeTaxAmount1()">
            </div> -->
        
      </div>
      <div class="col-lg-1 col-md-1 col-sm-1">
         <span class="font-bold">Tax Amount</span>
         <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
          <input type="text" class="form-control bg-secondary" placeholder="0.00" readonly
            style="text-align: right;height:40px;" formControlName="taxamount1"
            [(ngModel)]="taxamount1" (input)="OnChangeTaxAmount1()">
        </div>
      </div>
      
      <div class="col-12 col-lg-1 col-md-1 col-sm-12">
        <span class="font-bold">Total Amount</span>
        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="form-control bg-secondary" placeholder="0.00"
            style=" text-align: right;height:40px;" formControlName="totalamount"
            [(ngModel)]="totalamount" (input)="prodtotalcal()">
        </div>
      </div>
     

      <div class="col-lg-1 col-md-1 col-sm-1">
        <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-8">
          <button type="submit" class="btn-success ms-7" (click)="productSubmit()">+</button>&nbsp;
        </div>
      </div>

      <!-- <div class="col-12 col-lg-1 col-md-1 col-sm-12 "><span class="font-bold text-center ms-5">Action</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <button type="submit" class="btn-success" (click)="productSubmit()">+</button>&nbsp;
        </div>
