<div class="card">
  <div class="row ms-3 mt-3 d-flex align-items-center">
    <div class="col">
      <h3 class="card-title">Sales Team</h3>
    </div>

    <div class="col me-3">
      <button type="button" class="btn-primary btn-sm text-white me-4 float-end" data-bs-toggle="modal"
        data-bs-target="#myModaladd">
        <i class="fas fa-plus"></i>&nbsp;  Add Sales Team
      </button>
    </div>
  </div>
  <div class="table-responsive">
    <div class="mx-3 p-3 pt-0 pb-0 bg-white rounded table-responsive">
      <table id="salesteam_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
        <thead>
          <tr class="fw-bold fs-7 text-nowrap bg-lightblue text-blue align-middle">
            <th>S.NO </th>
            <th>Branch</th>
            <th>Team </th>
            <th>Description</th>
            <th class="text-center">No Of Enquires Assigned</th>
            <th class="text-center">Details</th>
            <th class="text-center">Action</th>
          </tr>
        </thead>
        <tbody>
          <tr *ngFor="let data of salesteam_list; let i=index;">
            <td>{{i+1}}</td>
            <td>{{data.branch_name}}</td>
            <td>{{data.campaign_title}}</td>
            <td>{{data.campaign_description}} </td>
            <td class="text-center">{{data.assigned_total}}</td>
            <td class="text-center text-nowrap">
              <label title="Product Details" data-bs-toggle="modal" data-bs-target="#myModalUpdateimage9"
                (click)="Details(data.parameter,data.campaign_gid)">
                <i class="fas fa-question-circle fa-5x text-center text-primary fs-2"></i>
              </label>
            </td>
            <td class="text-center text-nowrap">

              <button title="Edit" type="button" class="btn btn-icon btn-sm bg-success me-2"
                (click)="openModaledit(data.campaign_gid)" data-bs-toggle="modal" data-bs-target="#myModaledit"><span
                  class="fa-solid fa-pencil text-white  fs-6"></span></button>
              <button title="Employee Asssign" type="button" (click)="openModalemployeelist(data)"
                data-bs-toggle="modal" data-bs-target="#myModalempassign"
                class="btn btn-icon btn-sm background-color5 me-2"><span
                  class="fa-solid fa-users text-white  fs-6"></span></button>
              <button title="Manager Asssign" type="button" (click)="openModalmanagerlist(data)" data-bs-toggle="modal"
                data-bs-target="#myModalmanagerassign" class="btn btn-icon btn-sm background-color6 me-2"><span
                  class="fa-solid fa-user-tie text-white  fs-6"></span></button>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
<div class="modal fade" id="myModalUpdateimage9" tabindex="-1" data-bs-backdrop='static'
  aria-labelledby="editamlcategoryLabel" aria-hidden="true" data-bs-keyboard="false">
  <div class="modal-dialog modal-md modal-dialog-centered">
    <div class="modal-content  d-flex justify-content-center">
      <div class="card-header p-1 pb-1 pt-1 rounded-top bg-primary"
        style="display: flex; align-items: center; height: 40px;">
      </div>
      <div class="modal-body">
        <div class="table-responsive">
          <div class="mx-3 p-3 pt-0 pb-0 bg-white rounded">
            <table class="table table-striped table-row-bordered gy-5 gs-7 ">
              <thead>
                <tr class="fw-bold fs-7 text-nowrap bg-lightblue text-blue align-middle">
                  <th>S.No</th>
                  <th>Employee </th>
                  <th>Total</th>
                  <th>Prospect</th>
                  <th>Potential</th>
                  <th>Drop</th>
                  <th>Completed</th>
                </tr>
              </thead>
              <tbody>
                <tr *ngFor="let data of salesteamgrid_list; let i=index;" style="white-space: nowrap;">
                  <td>{{i+1}}</td>
                  <td>{{data.user}}</td>
                  <td>{{data.total}}</td>
                  <td>{{data.prospect}}</td>
                  <td>{{data.potential}}</td>
                  <td>{{data.drop_status}}</td>
                  <td>{{data.completed}}</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
        <div class="text-center my-4">
          <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal" style="background-color: red;">
            <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
          </button>&nbsp;
        </div>
      </div>
    </div>
  </div>
</div>


<form [formGroup]="reactiveForm" #form="ngForm">
  <div class="modal" id="myModaladd" data-bs-backdrop="static" data-bs-keyboard="false">
    <div class="modal-dialog w-200 ">
      <div class="modal-content">
        <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
          <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;<i class="fas fa-plus"></i>&nbsp;&nbsp;&nbsp;Add Sales
            Team</h2>
        </div>
        <div class="modal-body p-10">
          <div class="row"><span class="font-bold">Team<span class="ms-1 text-red">*</span></span>
            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
              <input formControlName="team_name" type="text" class="
                   form-control form-control-lg form-control-solid borderc-secondary
                   custom-input
                   mb-3 mb-lg-0
                   bg-white
                   custom-input
                 " [ngStyle]="{
                   'border-color':
                   team_name.invalid && team_name.touched
                       ? 'red'
                       : 'rgb(209, 209, 209)'
                 }" placeholder="Enter Team " name="team_name" style="height:40px;" />
            </div>
          </div>
          <div *ngIf="team_name.invalid && (team_name.dirty || team_name.touched)">
            <div *ngIf="team_name.errors?.['required']">
              <span style="color:red;">Team is required.</span>
            </div>
          </div>
          <br>
          <div class="col-12 col-lg-12 col-md-6 col-sm-12"> <span class="font-bold">Branch </span><span class="ms-1 text-red">*</span>
            <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <ng-select formControlName="branch_name" [items]="branch_list" required
                class="custom col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="branch_name"
                bindValue="branch_gid" placeholder="Select an option"></ng-select>

              <div *ngIf="branch_name.invalid && (branch_name.dirty || branch_name.touched)">
                <div *ngIf="branch_name.errors?.['required']">
                  <span style="color:red;"> Select Branch </span>
                </div>
              </div>
            </div>
            <br>
          </div>
          <div class="col-12 col-lg-12 col-md-6 col-sm-12"> <span class="font-bold">Team Manager </span><span class="ms-1 text-red">*</span>
            <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <ng-select formControlName="employee_name" [items]="Getemployee" required
                class="custom col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="employee_name"
                bindValue="employee_gid" placeholder="Select an option"></ng-select>
            </div>
            <div *ngIf="employee_name.invalid && (employee_name.dirty || employee_name.touched)">
              <div *ngIf="employee_name.errors?.['required']">
                <span style="color:red;"> Select Team </span>
              </div>
            </div>
            <br>
          </div>
          <div class="row"><span class="font-bold">Team Mail ID<span class="ms-1 text-red">*</span></span>
            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
              <input formControlName="mail_id" type="text" class="
    form-control form-control-lg form-control-solid
    borderc-secondary
    bg-white
    custom-input
    mb-3 mb-lg-0
  " [ngStyle]="{
    'border-color':
    mail_id.invalid && mail_id.touched
        ? 'red'
        : 'rgb(209, 209, 209)'
  }" placeholder="Enter Mail Id"
                pattern="^([a-z0-9-]+|[a-z0-9-]+([.][a-z0-9-]+)*)@([a-z0-9-]+\.[a-z]{2,20}(\.[a-z]{2})?|\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\]|localhost)$"
                name="Email Id" required  style="height:40px;"/>
            </div>
            <div *ngIf="
            mail_id.invalid &&
    (mail_id.dirty || mail_id.touched)
  " class="text-red">
              <div *ngIf="mail_id.errors?.['required']">
                Email Id is required.
              </div>
            </div>
            <div *ngIf="
            mail_id.invalid &&
  (mail_id.dirty || mail_id.touched)
" class="text-red">
              <div *ngIf="mail_id.errors?.['pattern']">
                Invalid Email!!.
              </div>
            </div>
          </div>
          <br>
          <div class="row"><span class="font-bold">Description</span>
            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
              <textarea type="text" class="
              form-control form-control-lg form-control-solid custom-input
              mb-3 mb-lg-0  borderc-secondary bg-white custom-input
            " placeholder="Team Description" formControlName="description" class="form-control" ></textarea>
            </div>

          </div>
          <br>
          <div class="text-center my-4">
            <button class="btn-success btn-sm text-white me-4" (click)="onsubmit()" [disabled]="reactiveForm.invalid"
              [ngClass]="{
            'primary-button': !reactiveForm.invalid,
            'secondary-button': reactiveForm.invalid
          }" data-bs-dismiss="modal">
              <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
            </button> &nbsp;&nbsp;
            <button class="btn-danger btn-sm text-white me-4" (click)="onclose()" data-bs-dismiss="modal">
              <i class="fas fa-close text-white"></i> &nbsp;Close
            </button>
          </div>

        </div>
      </div>
    </div>
  </div>
</form>
<form [formGroup]="reactiveFormEdit" >
  <div class="modal" id="myModaledit" data-bs-backdrop="static" data-bs-keyboard="false">
    <div class="modal-dialog w-200 ">
      <div class="modal-content">
        <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
          <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;<i class=""></i> &nbsp;&nbsp;Edit
            Team</h2>

        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
          <input type="text" class="
                form-control form-control-lg form-control-solid custom-input
                mb-3 mb-lg-0  borderc-secondary bg-white custom-input
              " hidden placeholder="campaign_gid" formControlName="campaign_gid" class="form-control" />
        </div>
        <div class="modal-body p-10">
          <div class="row"><span class="font-bold">Team Name<span class="ms-1 text-red">*</span></span>
            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">


              <input formControlName="campaign_title" type="text" class="
                   form-control form-control-lg form-control-solid borderc-secondary
                   custom-input
                   mb-3 mb-lg-0
                   bg-white
                   custom-input
                 " [ngStyle]="{
                   'border-color':
                   campaign_title.invalid && campaign_title.touched
                       ? 'red'
                       : 'rgb(209, 209, 209)'
                 }" placeholder="Enter Team " name="campaign_title"   style="height:40px;"/>
            </div>
          </div>
          <div *ngIf="campaign_title.invalid && (campaign_title.dirty || campaign_title.touched)">
            <div *ngIf="campaign_title.errors?.['required']">
              <span style="color:red;">Team is required.</span>
            </div>
          </div>
          <br>

          <div class="col-12 col-lg-12 col-md-6 col-sm-12"><span class="font-bold">Branch</span><span class="ms-1 text-red">*</span>
            <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <ng-select formControlName="campaign_location" [items]="branch_list" required
                class="custom col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="branch_name"
                bindValue="branch_gid" placeholder="Select an option"></ng-select>
            </div>
            <br>
          </div>

          <div class="row"><span class="font-bold">Team Mail ID<span class="ms-1 text-red">*</span></span>
            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">


              <input formControlName="campaign_mailid" type="text" class="
    form-control form-control-lg form-control-solid
    borderc-secondary
    bg-white
    custom-input
    mb-3 mb-lg-0
  " [ngStyle]="{
    'border-color':
    campaign_mailid.invalid && campaign_mailid.touched
        ? 'red'
        : 'rgb(209, 209, 209)'
  }" placeholder="Enter Mail Id"
                pattern="^([a-z0-9-]+|[a-z0-9-]+([.][a-z0-9-]+)*)@([a-z0-9-]+\.[a-z]{2,20}(\.[a-z]{2})?|\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\]|localhost)$"
                name="Email Id" required  style="height:40px;" />
            </div>
            <div *ngIf="
            campaign_mailid.invalid &&
    (campaign_mailid.dirty || campaign_mailid.touched)
  " class="text-red">
              <div *ngIf="campaign_mailid.errors?.['required']">
                Email Id is required.
              </div>
            </div>
            <div *ngIf="
            campaign_mailid.invalid &&
  (campaign_mailid.dirty || campaign_mailid.touched)
" class="text-red">
              <div *ngIf="campaign_mailid.errors?.['pattern']">
                Invalid Email!!.
              </div>
            </div>
          </div>
          <div class="row"><span class="font-bold">Description</span>
            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
              <textarea type="text" class="
              form-control form-control-lg form-control-solid custom-input
              mb-3 mb-lg-0  borderc-secondary bg-white custom-input
            " placeholder="Team Description" formControlName="description" class="form-control" ></textarea>
            </div>

            
          </div>
          <div class="text-center my-4">
            <button [disabled]="reactiveFormEdit.invalid" [ngClass]="{
                'primary-button': !reactiveFormEdit.invalid,
                'secondary-button': reactiveFormEdit.invalid
              }" data-bs-dismiss="modal" (click)="onupdate()">
              <i class="fas fa-check text-white fs-5 "></i> &nbsp;Update
            </button> &nbsp;&nbsp;
            <button class="btn-danger btn-sm text-white me-4" data-bs-dismiss="modal">
              <i class="fas fa-close text-white" routerLink="smr/SmrMstSalesteamSummary"></i> &nbsp;Close
            </button>
          </div>

        </div>
      </div>
    </div>
  </div>
</form>



<div class="modal modal-lg" id="myModalempassign" data-bs-backdrop="static" data-bs-keyboard="false">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
        <h2 class="mt-4 text-white fw-bold">
          <i class="bi bi-people fs-1 ms-6 me-4"></i>Marketing Team To Employee Assignment (Hierarchy Based)
        </h2>
      </div>
      <div class="text-center my-4">
        <span>
          <span class="fw-bold fs-1">Team:</span>
          <span class="text-red ms-4">{{teamname}}</span>
        </span>
        <span>
          <span class="fw-bold fs-1 ms-20">Branch:</span>
          <span class="text-red ms-4">{{branchname}}</span>
        </span>
      </div>
      <div class="container-fluid">
        <salesdual-list [sort]="keepSorted" [source]="source" [key]="key" [key1]="key1" [display]="display"
          [filter]="filter" [(destination)]="confirmed" height="265px" [format]="format" [disabled]="disabled">
        </salesdual-list>
      </div>
      <div class="text-center my-4">
        <div class="text-center my-4">
          <button class="btn-danger btn-sm text-white me-4" data-bs-dismiss="modal">
            <i class="fas fa-close text-white me-2"></i>Close
          </button>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="modal modal-lg" id="myModalmanagerassign" data-bs-backdrop="static" data-bs-keyboard="false">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
        <h2 class="mt-4 text-white fw-bold">
          <i class="bi bi-person-add fs-1 ms-6 me-4"></i>Assign Manager to Team
        </h2>
      </div>
      <div class="text-center my-4">
        <span>
          <span class="fw-bold fs-1">Team</span>
          <span class="text-red ms-4">{{teamname}}</span>
        </span>
        <span>
          <span class="fw-bold fs-1 ms-20">Branch</span>
          <span class="text-red ms-4">{{branchname}}</span>
        </span>
      </div>
      <div class="container-fluid">
        <salesmanager-list [sort1]="keepSorted1" [source1]="source1" [key2]="key2" [key3]="key3" [display1]="display1"
          [filter1]="filter1" [(destination)]="confirmed1" height="265px" [format1]="format1" [disabled1]="disabled1">
        </salesmanager-list>
      </div>
      <div class="text-center my-4">
        <div class="text-center my-4">
          <button class="btn-danger btn-sm text-white me-4" data-bs-dismiss="modal">
