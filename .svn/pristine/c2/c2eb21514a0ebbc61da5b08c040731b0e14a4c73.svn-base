<div class="card rounded-top">
  <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary " >
    <h3 class="fw-bolder  text-white"><i class="fas fa-eye fw-bolder fs-3"></i> &nbsp;&nbsp;&nbsp;View Quotation </h3>
</div>
<div class="m-5"  *ngFor="let m of quotationsummary_list">
           
            <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Ref. No. </span>
                 <p class="mt-3">{{m.quotation_gid}}</p>
                </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Quotation Date </span>
                    <p class="mt-3">{{m.quotation_date}}</p>
                </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Branch</span>
                    <p class="mt-3">{{m.branch_name}}</p>
                </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Customer Name</span>
                 <p class="mt-3">{{m.customer_name}}</p>
                </div>
            </div>
            <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Contact Person </span>
                    <p class="mt-3">{{m.contact_person}}</p>
                </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Contact Number</span>
                    <p class="mt-3">{{m.contact_no}}</p>
                </div>
                    <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Email Address </span>
                 <p class="mt-3">{{m.contact_mail}}</p>
                </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Customer Address </span>
                    <p class="mt-3">{{m.customer_address}}</p>
                </div>
            </div>
            <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >

              <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Sales Person Name </span>
                <p class="mt-3">{{m.user_firstname}}</p>
               </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Remarks</span>
                    <p class="mt-3">{{m.quotation_remarks}}</p>
                </div>
                <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Freight Terms</span>
                  <p class="mt-3">{{m.freight_terms}}</p>
              </div>
              <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Payment Terms</span>
               <p class="mt-3">{{m.payment_terms}}</p>
              </div>
               
                <!-- <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Customer Enquiry Ref. No. </span>
                    <p class="mt-3">{{m.customerenquiryref_number}}</p>
                </div> -->
               
            </div>
            <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >              
              <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Currency</span>
                <p class="mt-3">{{m.currency_code}}</p>
            </div>
            <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">Exchange Rate</span>
              <p class="mt-3">{{m.exchange_rate}}</p>
             </div>
            </div>
<div class="table-responsive">
    <div class="card card-body m-0 p-3 mb-5 bg-white rounded table-responsive">
      <table  class="table table-striped table-row-bordered gy-4 gs-2">
        <thead class="table_head">
          <tr class="text-white fw-bold fs-5 text-nowrap bg-lightblue text-blue">
            <th>S.No</th>
            <th>Product </th>
            <th>Product Group</th>
            <th>Product Code</th>
            <th>Unit</th>
            <th>Unit Price</th>
            <th>Quantity</th>
            <th>Discount (%)</th>
            <th>Tax</th>
            <th>Amount</th>
          </tr>
        </thead>
        <tbody>
          <tr *ngFor="let m of quotationsummary_list; let i=index;">
            <td>{{i+1}}</td>
            <td>{{m.product_name}}</td>
            <td>{{m.productgroup_name}}</td>
            <td>{{m.product_code}}</td>
            <td>{{m.productuom_name}}</td>
            <td>{{m.product_price}}</td>
            <td>{{m.quantity}}</td>
            <td>
              <div class="container">
                <div class="row">
                  {{m.discount_percentage}}
                </div>
                <div class="row">
                  {{m.discountamount}}
                </div>
              </div>
            </td>
            <td>
              <div class="row">
                <div class="col">
                  {{m.tax_name}}&nbsp;
                  {{m.tax_amount}}
                </div>
              </div>
            <td>
              {{m.price}}
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="card">
    <div class="d-flex flex-row m-6 mb-0 ms-1 flex-wrap">
      <div class="col-12 col-lg-3 col-md-6 col-sm-12"><span class="fw-bold">Payment Days</span>
        <p class="mt-3">{{m.payment_days}}</p>
      </div>

      <div class="col-12 col-lg-3 col-md-6 col-sm-12"><span class="fw-bold">Delivery Days</span>
        <p class="mt-3">{{m.delivery_days}}</p>
      </div>
    </div>
  </div>
  <div class="card">
  <div class="me-10">
    <div class="d-flex justify-content-end mt-1">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab ">Net Amount :</label>
        <span class="font-bold">{{m.total_price}}</span><span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Tax :</label>
        <span class="font-bold">{{m.tax_name}}</span> &nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Total with Tax :</label>
        <span class="font-bold">{{m.total_amount}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Add On Charges :</label>
        <span class="font-bold">{{m.addon_charge}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Additional Discount :</label>
        <span class="font-bold">{{m.additional_discount}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Freight Charges :</label>
        <span class="font-bold">{{m.freight_charges}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>
    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Buy/Back Charges :</label>
        <span class="font-bold">{{m.buyback_charges}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Packing Charges:</label>
        <span class="font-bold">{{m.packing_charges}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Insurance Charges :</label>
        <span class="font-bold">{{m.insurance_charges}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Round Off :</label>
        <span class="font-bold ">{{m.roundoff}}</span>&nbsp;&nbsp;<span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>

    <div class="d-flex justify-content-end mt-4">
      <div class="col-md-3 d-flex justify-content-between mt-4">
        <label class="tbl_lab">Grand Total :</label>
        <span class="font-bold">{{m.Grandtotal}}</span><span class="font-bold ms-12">{{m.currency_code}}</span>
      </div>
    </div>
    </div>
        </div>
    <div class="text-center my-4">
    <button class="btn-primary btn-sm text-white me-4" (click)="onback()">
        <i class="fas fa-arrow-left"></i> &nbsp; Back
      </button>
      </div>
</div>
