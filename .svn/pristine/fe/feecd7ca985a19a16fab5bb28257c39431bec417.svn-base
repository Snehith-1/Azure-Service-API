<div class="card rounded-top">
    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary" style="height:50px;">
      <h3 class="fw-bolder text-white"><i class="fas fa-eye fw-bolder fs-3"></i> &nbsp;&nbsp;&nbsp;View GRN
      </h3>
    </div>  


    <div *ngFor="let data of GetEditGrnInward_lists;">
        <!-- <div class="card-header bg-lightblue card-height55">
          <div class="card-title m-0 ">
            <h4 class="fw-bolder text-blue">Basic Info</h4>
          </div>
        </div> -->
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
          <div class="col-12 col-lg-3 col-md-6 col-sm-12 "><span class="fw-bold">GRN Ref No</span>
           <p class="mt-3">{{data.grn_gid}}</p>
          </div>
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"><span class="fw-bold">GRN Date </span>
           <p class="mt-3">{{data.grn_date | date:'dd-MM-yyyy'}}</p>
          </div>
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Vendor Name </span>
           <p class="mt-3">{{data.vendor_companyname}}</p>
           </div>
           <div class="col-12 col-lg-3 col-md-6 col-sm-12"><span class="fw-bold">Vendor Contact Number</span>
              <p class="mt-3">{{data.vendor_contact_person}}</p>
           </div> 
         </div>
         <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"><span class="fw-bold">Vendor Address </span>
            <p class="mt-3">{{data.address}}</p>
          </div>
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">PO Ref. No </span>
            <p class="mt-3">{{data.purchaseorder_gid}}</p>
           </div>
           <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Raised By </span>
              <p class="mt-3">{{data.user_checkername}}</p>
          </div> 
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">GRN Remarks</span>
            <p class="mt-3">{{data.grn_remarks }}</p>
           </div> 
         </div> 
         <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
            <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">GRN Ref</span>
              <p class="mt-3">{{data.grn_reference }}</p>
             </div>
            <div class="col-12 col-lg-3 col-md-6 col-sm-12 "> <span class="fw-bold">DC NO</span>
           <p class="mt-3" >{{data.dc_no | date:'dd-MM-yyyy'}}</p> 
         </div>
         <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Invoice Ref.No </span>
            <p class="mt-3">{{data.invoice_refno | date:'dd-MM-yyyy'}}</p>
        </div> 
        <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Vendor Contact person</span>
          <p class="mt-3">{{data.vendor_contact_person }}</p>
         </div> 
      </div>
      <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
        <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Email Address</span>
          <p class="mt-3">{{data.email_id }}</p>
         </div>
        <div class="col-12 col-lg-3 col-md-6 col-sm-12 "> <span class="fw-bold">Check by User</span>
       <p class="mt-3" >{{data.user_checkername}}</p> 
     </div>
     <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Approved By  </span>
        <p class="mt-3">{{data.user_approvedby}}</p>
    </div> 
    <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Approver Remarks</span>
      <p class="mt-3">{{data.reject_reason }}</p>
     </div> 
  </div>

  <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom" >
    <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">DC Date</span>
      <p class="mt-3">{{data.dc_date }}</p>
     </div>
    <div class="col-12 col-lg-3 col-md-6 col-sm-12 "> <span class="fw-bold">Invoice Date</span>
   <p class="mt-3" >{{data.invoice_date}}</p> 
 </div>
 <div class="col-12 col-lg-3 col-md-6 col-sm-12"> <span class="fw-bold">Priority</span>
    <p class="mt-3">{{data.priority_n}}</p>
</div> 

</div>
<div class="card m-3">

         
  <div class="col-12 col-lg-12 col-md-12 col-sm-12 fv-row mt-2">
        <table id="GetEditGrnInwardproduct_lists" class="table table-striped table-row-bordered gy-5 gs-7">
            <thead>
                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                
                <th>Product Group</th>
                <th>Product Code</th>
                <th>Product</th>
                <th>Unit</th>
                <th>Qty Ordered</th>
                <th>Qty Received</th>
                <th>Qty Adjusted</th>
                <th>Qty Rejected</th>
                <!-- <th>Remarks</th>
                <th>QC Remarks</th>
                <th>Location</th>
                <th>Bin</th> -->
            </tr>
            </thead>
            <tbody>
                <tr *ngFor="let data of GetEditGrnInwardproduct_lists;"> 
                
                <td>{{data.productgroup_name}}</td>
                <td>{{data.product_code}}</td>
                <td>{{data.product_name}}</td>
                <td>{{data.productuom_gid}}</td>
                <td>{{data.qty_ordered}}</td>
                <td>{{data.qty_received}}</td>
                <td>{{data.qty_grnadjusted}}</td>
                <td>{{data.qty_rejected}}</td>
                <!-- <td>{{data.product_remarks}}</td>
                <td>{{data.location_name}}</td>
                <td>{{data.bin_number}}</td>
                <td>{{data.qc_remarks}}</td> -->
                </tr>
            
            </tbody>
        </table>
    </div>
</div></div></div>

<div class="text-center my-4">
    <button class="btn-primary btn-sm text-white me-4 mt-3-center " routerLink="/pmr/PmrTrnGrninward">
        &nbsp;Back
    </button>
</div>
