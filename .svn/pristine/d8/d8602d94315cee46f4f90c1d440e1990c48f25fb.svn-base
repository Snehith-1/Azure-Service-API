<form [formGroup]="applyODForm" novalidate="novalidate" (ngSubmit)="onapplyod()">
    <div class="modal fade" id="applyod" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered odpopup">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Apply OD</h4>
                </div>
                <div class="modal-body odpopupbody">
                    <div class="d-flex flex-row flex-wrap">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>OD Date</b>
                            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="date" class="oddate-input form-control form-control-lg"
                                    formControlName="od_date" />
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>From</b>
                            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                <input type="time" class="od-input form-control form-control-lg"
                                    formControlName="od_fromhr" />
                            </div>
                        </div>

                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>To</b>
                            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex">
                                <input type="time" class="od-input form-control form-control-lg"
                                    formControlName="od_tohr" />
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Onduty Period</b>
                            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                <div
                                    class="form-check form-check-custom form-check-primary form-check-solid form-check-sm">
                                    <input class="form-check-input" type="radio" name="onduty_confirmed" value="Y" />
                                    <label class="form-check-label text-gray-700 text-nowrap"
                                        for="ondutyconfirmedFull">Full</label>

                                    <input class="form-check-input ms-6" type="radio" name="onduty_confirmed"
                                        value="N" />
                                    <label class="form-check-label text-gray-700 text-nowrap"
                                        for="ondutyconfirmedHalf">Half</label>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Session</b>
                            <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex">
                                <div
                                    class="form-check form-check-custom form-check-primary form-check-solid form-check-sm">
                                    <input class="form-check-input" type="radio" name="onduty_confirmed" value="Y" />
                                    <label class="form-check-label text-gray-700 text-nowrap"
                                        for="ondutyconfirmedFN">FN</label>

                                    <input class="form-check-input ms-6" type="radio" name="onduty_confirmed"
                                        value="N" />
                                    <label class="form-check-label text-gray-700 text-nowrap"
                                        for="ondutyconfirmedAN">AN</label>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-4">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Reason</b>
                            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                <textarea name="" id="" rows="5" class="inputfield form-control form-control-lg"
                                    formControlName="od_reason"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="text-center contain">
                    <button type="button" class="btn-success"><i class="bi bi-check-square fs-8 me-2"
                            (click)="onapplyod()"></i>Submit</button> &nbsp;
                    <button type="button" class="btn-danger" data-bs-dismiss="modal"><i
                            class="bi bi-x-square fs-8 me-2"></i>Close</button>
                </div>
            </div>
        </div>
    </div>
</form>

