<div class="card card-bordered">
    <div class="card-header  d-flex justify-content-center bg-primary" >
        <h1 class="card-title fw-bold fs-3 text-white">Quotation Report</h1>
    </div>
    <div class="row justify-content-center">
      <div class="col-md-2 mt-3 row">
        <label for="FromDate"><b>From Date:</b></label>
        <input type="date"   pattern="^(?!\s*$).+"  [(ngModel)]="from_date"
        class="form-control form-control-lg form-control-solid date-picker custom-input mb-3 mb-lg-0 bg-white
        custom-input date-picker " placeholder="DD-MM-YYYY" style="border: 1px solid black;" />
      </div>
      <div class="col-md-2 mt-3">
        <label for="ToDate"><b>To Date:</b></label>
        <input type="date"   pattern="^(?!\s*$).+"  [(ngModel)]="to_date"
        class="form-control form-control-lg form-control-solid date-picker custom-input mb-3 mb-lg-0 bg-white
        custom-input date-picker " placeholder="DD-MM-YYYY" style="border: 1px solid black;" />
      </div>
      <div class="col-md-1 mt-9 row">
        <button class="btn-primary btn-sm text-black me-1" (click)="onSearchClick()">Search</button>
      </div>
      <div class="col-md-1 mt-9 row">
        <button class="btn-primary btn-sm text-black ms-1" (click)="onrefreshclick()">Refresh</button>
      </div>
    </div>

    <div class="d-flex flex-wrap flex-row  ">
        <div class="col-7 card m-8 ms-5 shadow-lg p-3 mb-8 mt-8 bg-white rounded">

            <div class="me-1">
          <div class="card-header justify-content-center bg-primary" >
            <h3 class="card-title fs-3 fw-bold text-white"><i class="fa-solid fa-chart-column ms-1"style="font-size:20px; margin-right: 5px;"></i> Quotation Report Chart</h3>
          </div>
                <div class="card-body align-items-center position-relative ">

                    <div class="d-flex flex-center me-5 pt-2">
                        <div id="kt_card_widget_17_chart"> </div>
                    </div>

                    <div class="d-flex flex-wrap ">


                        <!-- <div class="d-flex   fw-semibold  align-items-center ">

                            <div class=" w-10px h-10px p-1 me-3" style="background-color: #7FC7D9"></div>

                            <div class="text-gray-500 flex-grow-1 me-4 fw-bold">Quotation</div>

                        </div> -->

                    </div>
                </div>
            </div>

            <div id="chart" style="width: 800px; margin-left: 40px;">
            </div>

        </div>
        <div class="col-12 col-lg-4 mt-20 mb-20 card m-3 ms-0 ms-lg-12 shadow-lg p-3  bg-white rounded">
            
            <div class="col-12  bg-white rounded">
                <div class="card-header justify-content-center bg-primary">
                    <h3 class="card-title fs-4 fw-bolder text-white"><i class="fa-regular fa-file" style="font-size: 18px;  margin-right: 5px;"></i>Quotation (Last 6 Months) </h3>
    
                </div>
                <div class="table-responsive">
                    <div class="card card-body p-3 mb-5 ">
                      <div class="mx-1 p-1  pb-0  table-responsive">
                        <table id="GetQuotationForLastSixMonths_List" class="table  table-striped table-row-bordered gy-5 gs-7 ">
                            <thead>
                                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-black align-middle">
                                    <th class ="text-start" >Year </th>
                                    <th class="text-start">Month </th>
                                    <th class="text-center">Order Count </th>
                                    <th class="text-end">Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr *ngFor="let data of GetQuotationForLastSixMonths_List; let i=index;">
                                    <td class ="text-start">{{data.year}}</td>
                                    <td class ="text-start">{{data.month}}</td>
                                    <td class="text-center">{{data.quotationcount}}</td>
                                    <td class="text-end">{{data.amount | number}}</td>
    
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>  


            




        </div>
    </div>
    <div class="col-12 ms-0 card shadow-lg p-3 mb-3 bg-white rounded">
    <div class="col-12 card  bg-white rounded">
        <div class="card-header justify-content-center bg-primary">
          <h3 class="card-title fs-4 fw-bolder text-white"><i class="fa-regular fa-file" style="font-size: 18px;  margin-right: 5px;"></i>Quotation (Last 6 Months) </h3>
      </div>
      <div class="table-responsive">
          <div class="card card-body p-3 mb-5 ">
            
            <table id="GetQuotationForLastSixMonths_List" class="table table-striped table-row-bordered gy-5 gs-7 ">
              <thead>
                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-black align-middle">
                  <th >Month </th>
                  <th class="text-center">Order Count </th>
                  <th class="text-end" >Amount</th>
                  <th class ="text-center">Quotation Details</th>
                </tr>
              </thead>
              <tbody>
                <tr *ngFor="let data of GetQuotationForLastSixMonths_List; let i=index;">
                 
                  
                  <td>{{data.month}}</td>
                  <td class="text-center" >{{data.quotationcount}}</td>
                  <td class="text-end">{{data.amount| number}}</td>
                  <td class="text-center text-nowrap">
                    <label title=" Details" data-bs-toggle="modal" data-bs-target="#myModalUpdateimage9"
                        (click)="ondetail(data.month,data.year,data.quotation_gid,data.parameter)">
                        <i class="fas fa-question-circle fa-5x text-primary fs-2 text-center"></i>
                    </label>
                </td>
                </tr>
              </tbody>
            </table>
          </div>
      </div>
    </div>

    <div class="modal fade" id="myModalUpdateimage9"  data-bs-backdrop='static'
        aria-labelledby="editamlcategoryLabel" aria-hidden="true" data-bs-keyboard="false">
        <div class="modal-dialog modal-md modal-dialog-centered modal-xl" >
            <div class="modal-content  d-flex justify-content-center">
                <div class=" card-header p-1 pb-1 pt-1 rounded-top bg-border " style="background-color:rgb(25, 167, 206); "
                style="display: flex; align-items: center; height: 40px;">
                <h3 style="color: white; font-weight: bold; text-align: left; width: 100%; margin-left: 10px;">
                    {{month}}</h3>
                   
                </div>
                <div class="modal-body">
                  <div class="table-responsive">
                    <div class="mx-3 p-3 pt-0 pb-0 bg-white rounded let data of GetQuotationSummary" >
                      <table class="table table-striped table-row-bordered gy-5 gs-7 ">
                        <thead>
                          <tr class="fw-bold fs-7 text-nowrap bg-lightblue text-blue align-middle">
                            <th>S.No</th>
                                <th class="text-start"> Order Date</th>
                                <th class="text-start">Customer Name</th>
                                <th class="text-start"> Contact Details</th>
                                <th class="text-end"> Amount</th>
                                <th class="text-start"> Quotation Status</th>
                                <th class="text-start">Created By</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr *ngFor="let data of GetQuotationSummary; let i=index;" style="white-space: nowrap;">
                            <td>{{i+1}}</td>
                              <td class="text-start">{{data.quotation_date}}</td>
                              <td class="text-start">{{data.customer_name}}</td>
                              <td class="text-start">{{data.contact_details}}</td>
                              <td class="text-end">{{data.grandtotal_l}}</td>
                              <td  class="text-start" >{{data.quotation_status}}</td>
                              <td class="text-start">{{data.created_by}}</td>

                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
                    <div class="text-center my-4">
                        <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal"
                            style="background-color: red;">
                            <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
                        </button>&nbsp;
                    </div>
                </div>
            </div>
        </div>
    </div>
  
                 
