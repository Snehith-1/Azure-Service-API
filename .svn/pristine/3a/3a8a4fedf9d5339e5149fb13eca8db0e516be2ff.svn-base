<div class="card shadow-lg rounded-top">
  <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
    <h3 class="fw-bolder text-white">
      <i class="fa fa-user fw-bolder fs-3 ms-5 me-5"></i>My Profile
    </h3>
  </div>

  <div class="back-icon">
    <i class="bi bi-arrow-left-square-fill" (click)="back()"></i>
  </div>

  <div class="card">
    <div class="card-body">
      <div class="row">
        <div class="col-lg-3 col-md-12 col-sm-12">
          <div class="card shadow-sm p-3 ">
            <div class="col-lg-12 col-md-6 col-sm-12 text-center symbol symbol-150px symbol-circle">
              <img src="assets\media\images\images.png" width="120%" height="200">
            </div>
            <div class="text-center mt-6">
              <label for="">{{emp_code}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_name}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_designation}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_department}}</label>
            </div>
          </div>
        </div>

        <div class="col-lg-9 col-md-12 col-sm-12">
          <ul class="nav nav-tabs nav-line-tabs nav-line-tabs-20x mb-8 fs-6">
            <li class="nav-item">
              <a class="nav-link active" data-bs-toggle="tab" href="#kt_tab_pane_4"><b>Personal Details</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_5"><b>Change Password</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_6"><b>Work Experience</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7"><b>Nomination</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_8"><b>Statutory</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_9"><b>Emergency Contact</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_10"><b>Dependent</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_11"><b>Education</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_12"><b>Policy</b></a>
            </li>
          </ul>

          <div class="card shadow-sm p-3">
            <div class="tab-content" id="myTabContent">
              <!-- Personal details -->
              <div class="tab-pane fade show active ms-5" id="kt_tab_pane_4" role="tabpanel">
                <form [formGroup]="reactiveForm7Edit" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa fa-info-circle fs-4 me-5"></i>Personal Details</h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>First Name</b><span style="color:red"> *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="firstedit_name" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': firstedit_name.invalid && firstedit_name.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter First Name" pattern="^(?!\s*$).+"
                            [class.is-invalid]="firstedit_name.invalid && (firstedit_name.dirty || firstedit_name.touched)"
                            name="firstedit_name" required />
                          <div *ngIf="firstedit_name.invalid && (firstedit_name.dirty || firstedit_name.touched)"
                            class="invalid-feedback">
                            <div *ngIf="firstedit_name.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              First name is required</div>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Last Name</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="lastedit_name" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': lastedit_name.invalid && lastedit_name.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Last Name" pattern="^(?!\s*$).+" name="lastedit_name" required />
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Date Of Birth</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="dateof_birthedit" type="date"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': dateof_birthedit.invalid && dateof_birthedit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter First Name" pattern="^(?!\s*$).+" name="dateof_birthedit" required />
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Blood Group</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <select
                            class="form-select form-select-solid form-select-lg borderc-secondary bg-white custom-input select2"
                            formControlName="blood_groupedit">
                            <option value="" disabled selected>Select Blood Group</option>
                            <option *ngFor="let blood_groupedit of bloodgroup_list"
                              [value]="blood_groupedit.blood_groupedit_gid">
                              {{blood_groupedit.bloodgroup_name}}
                            </option>
                          </select>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Gender</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-6">
                          <input class="form-check-input" formControlName="gender_edit" type="radio" value="male"
                            value="MALE" />
                          <label for="html" class="ms-3"><b>Male</b></label>
                          <input class="form-check-input ms-10" formControlName="gender_edit" type="radio"
                            value="female" value="FEMALE" />
                          <label for="html" class="ms-3"><b>Female</b></label>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Mobile Number</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="mobile_edit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': mobile_edit.invalid && mobile_edit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Mobile Number" name="mobile_edit" maxlength="10" required />
                        </div>
                        <div *ngIf="mobile_edit.invalid && (mobile_edit.dirty || mobile_edit.touched)">
                          <div *ngIf="mobile_edit.errors?.['pattern']">
                            <span
                              style="color: red; width: 200px; margin-top: 5px; margin-left: 8px; font-size: 11px;">*
                              Maximum 10 digits</span>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Personal Number</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="personal_noedit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': personal_noedit.invalid && personal_noedit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Personal Number" name="personal_noedit" maxlength="10" required />
                        </div>
                        <div *ngIf="personal_noedit.invalid && (personal_noedit.dirty || personal_noedit.touched)">
                          <div *ngIf="personal_noedit.errors?.['pattern']">
                            <span
                              style="color: red; width: 200px; margin-top: 5px; margin-left: 8px; font-size: 11px;">*
                              Maximum 10 digits</span>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Qualification</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="qualification_edit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': qualification_edit.invalid && qualification_edit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Qualification" pattern="^(?!\s*$).+" name="qualification_edit"
                            required />
                        </div>
                      </div>

                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Experience</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="experience_edit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': experience_edit.invalid && experience_edit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Experience" pattern="^(?!\s*$).+" name="experience_edit" required />
                        </div>
                      </div>
                    </div>

                    <div class="text-center my-4 mt-10">
                      <button (click)="detailsupdate()" [disabled]="reactiveForm7Edit.invalid"
                        [ngClass]="{'primary-button': !reactiveForm7Edit.invalid,'secondary-button': reactiveForm7Edit.invalid}"
                        data-bs-dismiss="modal">
                        <i class="fas fa-check text-white fs-5 me-4"></i>Update
                      </button>
                    </div>
                  </div>
                </form>
              </div>

              <!--Change Password-->
              <div class="row tab-pane fade ms-5" id="kt_tab_pane_5" role="tabpanel">
                <form [formGroup]="reactiveForm8Edit" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa fa-key fs-4 me-5"></i>Change Password
                      </h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Current Password</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="curredit_pwd" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': curredit_pwd.invalid && curredit_pwd.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Current Password" pattern="^(?!\s*$).+"
                            [class.is-invalid]="curredit_pwd.invalid && (curredit_pwd.dirty || curredit_pwd.touched)"
                            name="curredit_pwd" required />
                          <div *ngIf="curredit_pwd.invalid && (curredit_pwd.dirty || curredit_pwd.touched)"
                            class="invalid-feedback">
                            <div *ngIf="curredit_pwd.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Current password is required</div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>New Password</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="newedit_pwd" type="password"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': newedit_pwd.invalid && newedit_pwd.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter New Password" pattern="^(?!\s*$).+"
                            [class.is-invalid]="newedit_pwd.invalid && (newedit_pwd.dirty || newedit_pwd.touched)"
                            name="newedit_pwd" required />
                          <div *ngIf="newedit_pwd.invalid && (newedit_pwd.dirty || newedit_pwd.touched)"
                            class="invalid-feedback">
                            <div *ngIf="newedit_pwd.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              New password is required</div>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Confirm Password</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="confedit_pwd" type="password"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': confedit_pwd.invalid && confedit_pwd.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Confirm Password" pattern="^(?!\s*$).+"
                            [class.is-invalid]="confedit_pwd.invalid && (confedit_pwd.dirty || confedit_pwd.touched)"
                            name="confedit_pwd" required />
                          <div *ngIf="confedit_pwd.invalid && (confedit_pwd.dirty || confedit_pwd.touched)"
                            class="invalid-feedback">
                            <div *ngIf="confedit_pwd.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Confirm password is required</div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="text-center my-4 mt-10">
                      <button (click)="passwordupdate()" [disabled]="reactiveForm8Edit.invalid" [ngClass]="{
                                          'primary-button': !reactiveForm8Edit.invalid,
                                          'secondary-button': reactiveForm8Edit.invalid
                                        }" data-bs-dismiss="modal">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Update
                      </button> &nbsp;&nbsp;
                    </div>
                  </div>
                </form>
              </div>

              <!--Work Experience-->
              <div class="tab-pane fade ms-5" id="kt_tab_pane_6" role="tabpanel">
                <form [formGroup]="reactiveForm6" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa-solid fa-briefcase fs-4 me-5"></i>Work Experience
                      </h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Employee's Previous Company</b><span
                          style="color:red"> *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="empl_prevcomp" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': empl_prevcomp.invalid && empl_prevcomp.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Previous Company" pattern="^(?!\s*$).+"
                            [class.is-invalid]="empl_prevcomp.invalid && (empl_prevcomp.dirty || empl_prevcomp.touched)"
                            name="empl_prevcomp" required />
                          <div *ngIf="empl_prevcomp.invalid && (empl_prevcomp.dirty || empl_prevcomp.touched)"
                            class="invalid-feedback">
                            <div *ngIf="empl_prevcomp.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Previous company name is required</div>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Employee Code</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="empl_code" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': empl_code.invalid && empl_code.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Employee Code" pattern="^(?!\s*$).+"
                            [class.is-invalid]="empl_code.invalid && (empl_code.dirty || empl_code.touched)"
                            name="empl_code" required />
                          <div *ngIf="empl_code.invalid && (empl_code.dirty || empl_code.touched)"
                            class="invalid-feedback">
                            <div *ngIf="empl_code.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Employee code is required</div>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Previous Occupation</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="prev_occp" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': prev_occp.invalid && prev_occp.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Previous Occupation" pattern="^(?!\s*$).+"
                            [class.is-invalid]="prev_occp.invalid && (prev_occp.dirty || prev_occp.touched)"
                            name="prev_occp" required />
                          <div *ngIf="prev_occp.invalid && (prev_occp.dirty || prev_occp.touched)"
                            class="invalid-feedback">
                            <div *ngIf="prev_occp.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Previous occupation is required</div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Date of Joining</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="date_ofjoining" type="date"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': date_ofjoining.invalid && date_ofjoining.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Joining Date" pattern="^(?!\s*$).+"
                            [class.is-invalid]="date_ofjoining.invalid && (date_ofjoining.dirty || date_ofjoining.touched)"
                            name="date_ofjoining" required />
                          <div *ngIf="date_ofjoining.invalid && (date_ofjoining.dirty || date_ofjoining.touched)"
                            class="invalid-feedback">
                            <div *ngIf="date_ofjoining.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Joining date is required</div>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Date of Relieving</b><span style="color:red">
                          *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="date_ofreleiving" type="date"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': date_ofreleiving.invalid && date_ofreleiving.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Relieving Date" pattern="^(?!\s*$).+"
                            [class.is-invalid]="date_ofreleiving.invalid && (date_ofreleiving.dirty || date_ofreleiving.touched)"
                            name="date_ofreleiving" required />
                          <div *ngIf="date_ofreleiving.invalid && (date_ofreleiving.dirty || date_ofreleiving.touched)"
                            class="invalid-feedback">
                            <div *ngIf="date_ofreleiving.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Relieving date is required</div>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><b>Working Period</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="work_period" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input"
                            placeholder="Enter Working Period" name="work_period" />
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Reason To Leave Previous Company</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="reason" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input"
                            placeholder="Reason To Leave" name="reason" />
                        </div>
                      </div>

                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Department</b><span style="color:red"> *</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="department" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': department.invalid && department.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Department" pattern="^(?!\s*$).+"
                            [class.is-invalid]="department.invalid && (department.dirty || department.touched)"
                            name="department" required />
                          <div *ngIf="department.invalid && (department.dirty || department.touched)"
                            class="invalid-feedback">
                            <div *ngIf="department.errors?.['required']"
                              style="color: red; width: 200px; margin-top: 1px; margin-left: 8px; font-size: 11px;">*
                              Department is required</div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-6 col-md-6 col-sm-12"><b>Reporting To</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="report" type="text"
