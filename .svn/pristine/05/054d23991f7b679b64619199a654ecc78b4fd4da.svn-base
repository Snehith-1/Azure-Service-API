<div class="card">
    <div class="row ms-3 mt-3 d-flex align-items-center">
        <div class="col">
            <h3 class="card-title">My Leads - Potentials</h3>
        </div>
        <div class="col me-5">

        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnMycampaign">
            <div class="cardHead radius-10 border-start border-0 border-3 border-violet">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Today Task</h5>
                            <div *ngFor="let MyTodayTaskCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{MyTodayTaskCount_List.todaytask_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-violet text-white ms-auto"><i
                                class="bi bi-calendar3"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnUpcomingMeetings">
            <div class="cardHead radius-10 border-start border-0 border-3 border-brown">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary"> Upcoming </h5>
                            <div *ngFor="let MyUpcomingTaskCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{MyUpcomingTaskCount_List.upcoming_counts}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-brown text-white ms-auto"><i
                                class="bi bi-arrow-up-right"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnNewtask">
            <div class="cardHead radius-10 border-start border-0 border-3 border-darkblue">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">New</h5>
                            <div *ngFor="let MyNewTaskCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{MyNewTaskCount_List.newlead_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkblue text-white ms-auto"><i
                                class="bi bi-asterisk"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnProspects">
            <div class="cardHead radius-10 border-start border-0 border-3 border-darkorange">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Prospects</h5>
                            <div *ngFor="let ProspectsCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{ProspectsCount_List.prospects_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-purple text-white ms-auto"><i
                                class="bi bi-browser-edge"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnPotentials">
            <div class="cardHead radius-10 border-start border-0 border-3 border-orange"
                style="background-color: #ffead4;">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Potentials</h5>
                            <div *ngFor="let PotentialsCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{PotentialsCount_List.potential_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkgreen text-white ms-auto"><i
                                class="bi bi-clipboard-fill"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnCompleted">
            <div class="cardHead radius-10 border-start border-0 border-3 border-darkgreen">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Completed</h5>
                            <div *ngFor="let CompletedCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{CompletedCount_List.completed_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkgreens text-white ms-auto"><i
                                class="bi bi-check-lg"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnDropleads">
            <div class="cardHead radius-10 border-start border-0 border-3 border-purple">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Drop</h5>
                            <div *ngFor="let DropCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{DropCount_List.drop_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkoranges text-white ms-auto"><i
                                class="bi bi-hand-thumbs-down-fill"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnAllleads">
            <div class="cardHead radius-10 border-start border-0 border-3 border-blue">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">All</h5>
                            <div *ngFor="let AllCount_List of MyLeadsCount_List">
                                <h4 class="my-1 text-info">{{AllCount_List.allleads_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkblues text-white ms-auto"><i
                                class="bi bi-columns-gap"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
   
        <div class="table-responsive">
            <div class="card card-body m-3 p-3 mb-5 bg-white rounded">
                <table id="potentiallist" class="table table-striped table-row-bordered border-bottom gs-7">
                    <thead>
                        <tr class="fw-bolder fs-5 text-nowrap bg-lightblue text-blue align-middle">
                            <th>S.No</th>
                            <th>Team </th>
                            <th>Customer</th>
                            <th>Customer Type</th>
                            <th>Contact</th>
                            <th class="text-center">Call Response</th>
                            <th class="text-center">Remarks</th>
                            <th class="text-center">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr *ngFor="let data of potentiallist; let i=index;" class="align-middle">
                            <td>{{i+1}}</td>
                            <td style="white-space: nowrap;">{{data.campaign_title}}</td>
                            <td style="white-space: nowrap;">{{data.leadbank_name}}</td>
                            <td>{{data.customer_type}}</td>
                            <td style="white-space: nowrap;">{{data.contact_details}}</td>
                            <!-- <td>{{data.internal_notes}}</td> -->
                            <td class="text-center text-nowrap">
                                <button type="button" class="btn btn-secondary position-relative"
                                    style="width: 40px; height: 40px;"
                                    (click)="popcall(data.leadbank_gid,data.leadbank_name)" data-bs-toggle="modal"
                                    data-bs-target="#myModalcallresponse">
                                    <i class="fas fa-phone fs-2" style="color: rgb(37, 35, 35); font-size: 10px;"></i>
                                    <div *ngIf="data.call_count > 0">
                                        <span
                                            class="position-absolute top-1 start-100 translate-middle badge rounded-pill bg-success">
                                            {{data.call_count}}
                                        </span>
                                    </div>
                                </button>
                            </td>
                            <!-- <td class="text-center text-nowrap">
                                <button title="View Details" type="button"
                                    (click)="popmodal(data.internal_notes,data.leadbank_name)" data-bs-toggle="modal"
                                    data-bs-target="#myModalUpdateimage3" class="btn btn-icon btn-sm bg-secondary">
                                    <i class="fa-solid fa-question  fs-4" style="color:grey; font-size: 20px;"></i>
                                </button>
                            </td> -->
                            <td class="text-center text-nowrap">
                                <label title="Remarks" data-bs-toggle="modal" data-bs-target="#myModalUpdateimage3"
                                    (click)="popmodal(data.internal_notes,data.leadbank_name)">
                                    <i class="fas fa-question-circle fa-5x text-primary fs-2"></i>
                                </label>
                            </td>
                            <td class="text-center text-nowrap">
                                <button title="360 View" type="button"
                                    class="btn btn-icon btn-sm  me-2 bg-gradient-darkblue">
                                    <i class="bi bi-arrow-repeat" style="font-size:14px;font-weight:bolder;"
                                        (click)="Onopen(data.leadbank_gid,data.lead2campaign_gid)"></i>
                                </button>
                                <button (click)="openModallog4(data)" title="Log" data-bs-toggle="modal" type="button"
                                    data-bs-target="#mylog3" class="btn btn-icon btn-sm bg-success me-2"><span
                                        class="fa fa-phone text-white  fs-6"></span>
                                </button>
                                <button (click)="openModallog3(data)" title="Schedule" data-bs-toggle="modal"
                                    type="button" data-bs-target="#myschedule"
                                    class="btn btn-icon btn-sm bg-gradient-darkoranges  me-2"><span
                                        class="fa fa-clock text-white  fs-6"></span>
                                </button>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
   
</div>
<!----Remarks Potential popup-->
<div class="modal fade" id="myModalUpdateimage3" tabindex="-1" data-bs-backdrop='static'
    aria-labelledby="editamlcategoryLabel" aria-hidden="true" data-bs-keyboard="false">
    <div class="modal-dialog modal-md modal-dialog-centered">
        <div class="modal-content">
            <div class="card-header p-1 pb-1 pt-1 rounded-top bg-primary"
                style="display: flex; align-items: center; height: 40px;">
                <h3 style="color: white; font-weight: bold; text-align: left; width: 100%; margin-left: 10px;">
                    {{leadbank_name}}</h3>
            </div>


            <div class="modal-body">
                <div class="col-12 col-lg-3 col-md-6 col-lg-12">
                    <span class="fw-bold">Remarks: </span>
                    <p class="mt-2 wrap-text" style="max-width: 100%; overflow: auto; max-height: 200px;">
                        {{internal_notes}}</p>
                </div>

                <div class="text-center my-4">
                    <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal"
                        style="background-color: red;">
                        <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
                    </button>&nbsp;
                </div>
            </div>
        </div>
    </div>
</div>
<!-- -------Call Response------ -->
<form [formGroup]="reactiveFormCallresponse" #form="ngForm">
    <div class="modal" id="myModalcallresponse" data-bs-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="card-header p-1 pb-1 pt-1 rounded-top bg-primary"
                    style="display: flex; align-items: center; height: 40px;">
                    <h3 style="color: white; font-weight: bold; text-align: left; width: 100%; margin-left: 10px;">
                        {{leadbank_name}}</h3>
                </div>


                <div class="modal-body">
                    <div class="col-12 col-lg-3 col-md-6 col-lg-12">
                        <span class="fw-bold">Call Response: </span><br /><br />
                        <ng-container *ngFor="let response of callresponse_list; let i = index">
                            <div style="margin-bottom: 5px;padding: 5px;" class="panel bg-white">
                                {{response.created_date}}
                                <div style="margin-bottom: 3px;margin-top: 2px;">
                                    <div class="panel-body">{{ response.call_remarks }}</div>
                                </div>
                            </div>
                             <!-- Add a horizontal line after each schedule log -->
                             <hr style="margin-top: 5px; margin-bottom: 5px;">
                        </ng-container>
                    </div>

                    <div class="text-center my-4">
                        <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal"
                            style="background-color: red;">
                            <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
                        </button>&nbsp;
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
