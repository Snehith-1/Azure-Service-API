                                <label for="exampleFormControlTextarea1" class="form-label"><b>Call
                                        Feedback</b></label>
                                <textarea style="resize: none;" class="form-control" formControlName="call_feedback"
                                    pattern="^(?!\s*$).+" placeholder="" id="exampleFormControlTextarea1"
                                    rows="3"></textarea>
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">

                        <!-- <div class="col-lg-6 col-md-6 col-sm-6 fv-row mt-2">
                            <span class="font-bold"><b>Product Name</b><span class="ms-1 text-red"></span></span>

                            <ng-select style=" width: 355px;" formControlName="product_name" [class.is-invalid]="
                    product_name.invalid && (product_name.dirty || product_name.touched)"
                                [ngClass]="{'invalid-border': product_name.invalid && product_name.touched}"
                                [items]="product_list" class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
