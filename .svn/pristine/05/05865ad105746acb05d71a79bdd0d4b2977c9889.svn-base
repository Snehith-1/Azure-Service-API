<!-- begin::Add PO -->
<div class="card m-3">
  <!-- begin::Heading -->
  <div class="card-header bg-lightblue card-height55">
    <div class="card-title m-0">
      <h5 class="fw-bolder text-blue"> Direct Purchase Order</h5>
    </div>
  </div>
  <!-- end::Heading -->

  <!-- begin:: PO Add Form-->
  <form [formGroup]="POAddForm" #form="ngForm" (ngSubmit)="onSubmit()">
    <div class="d-flex flex-row m-6 flex-wrap">

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Ref.No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input "
            placeholder="PO Ref. No."  class="form-control" readonly />
        </div>
      </div>




      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white "
            placeholder="New Purchase Order" formControlName="po_no" class="form-control" />
        </div>
      
      </div>

    

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Date</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="date"
            class="form-control form-control-lg form-control-solid date-picker mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="DD-MM-YYYY" />
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch Name<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select formControlName="branch_name" (change)="OnChangeBranch()"
            [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
            [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="branch_list" required
            class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="branch_name"
            bindValue="branch_gid" placeholder="Select an option" [(ngModel)]="mdlBranchName">
          </ng-select>
          <div *ngIf="
                            product_code.invalid &&
                                (product_code.dirty || product_code.touched)
                              " class="invalid-feedback">
            <div *ngIf="product_code.errors?.['required']">
              Product Code is required.
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Name<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select formControlName="vendor_companyname" (change)="OnChangeVendor()"
            [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
            [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="vendor_list" required
            class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="vendor_companyname"
            bindValue="vendor_gid" placeholder="Select an option" [(ngModel)]="mdlVendorName">
            <option *ngFor="let vendor of vendor_list" [value]="vendor.vendor_gid">
              {{vendor.vendor_companyname}}</option>
          </ng-select>
          <div *ngIf="
                            product_code.invalid &&
                                (product_code.dirty || product_code.touched)
                              " class="invalid-feedback">
            <div *ngIf="product_code.errors?.['required']">
              Vendor Name is required.
            </div>
          </div>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Despatch To Branch<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <ng-select formControlName="dispatch_name"
            [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
            [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="dispatch_list"
            required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="dispatch_name"
            bindValue="branch_gid" placeholder="Select an option" [(ngModel)]="mdlDispatchName">
            <option *ngFor="let dispatch of dispatch_list" [value]="dispatch.branch_gid">
              {{dispatch.dispatch_name}}</option>
          </ng-select>
          <div *ngIf="
                            product_code.invalid &&
                                (product_code.dirty || product_code.touched)
                              " class="invalid-feedback">
            <div *ngIf="product_code.errors?.['required']">
              Despatch Branch Name is required.
            </div>
          </div>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Mobile</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" readonly formControlName="contact_number" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact Person</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" readonly formControlName="contact_person" class="form-control" />
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Fax</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Vendor Fax" formControlName="vendor_fax" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Email </b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" readonly formControlName="email_address" class="form-control" />
        </div>
        
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Address</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" readonly formControlName="vendor_address" class="form-control"></textarea>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchange Rate</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2 ">
          <input type="number" style="text-align:right;"
          class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input "
            placeholder="0.00"formControlName="exchange_rate" class="form-control" />
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Ship Via</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Ship Via" formControlName="ship_via" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Payment Terms" formControlName="payment_terms" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Freight Terms" formControlName="freight_terms" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Delivery Location</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Delivery Location" formControlName="delivery_location" class="form-control" />
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <!-- <select class="form-select form-select-solid bg-white form-select-lg borderc-secondary custom-input select2"
            formControlName="currency_code" (change)="OnChangeCurrency($event)" (change)="onCurrencyCodeChange($event)">
            <option value="" disabled selected>Select Currency</option>
            <option *ngFor="let currency of currency_list" [value]="currency.currencyexchange_gid">
              {{currency.currency_code}}</option>
          </select> -->
          <ng-select formControlName="currency_code"
          [items]="currency_list" (change)="OnChangeCurrency()"
          required class="custom col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="currency_code"
          bindValue="currencyexchange_gid" placeholder="Select an option" [(ngModel)]="mdlCurrencyName">
          <option *ngFor="let currency of currency_list" [value]="currency.currencyexchange_gid">
            {{currency.currency_code}}</option>
        </ng-select>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping Address</b>
        <div class="col-lg-11 col-12 col-md-12 col-sm-12 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Shipping Address" formControlName="Shipping_address" class="form-control"
            style="height: 20px;;"></textarea>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Cover Note</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" formControlName="pocovernote_address" class="form-control">Sub: Purchase Order.
                        Ref: As per your Quotation.
                        Please print the above said items with the following descriptions.</textarea>
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Priority</b>
       
        <div class="d-flex justify-content-start mt-4 ms-2">
          <div class="form-check form-check-custom form-check-primary form-check-solid form-check-sm">
            <input class="form-check-input" type="radio" name="priority" formControlName="priority_n" value="Y"
              (change)="showTextBox($event)" />
            <label class="form-check-label text-gray-700 text-nowrap" for="priorityYes">Yes</label>
          </div>
          <div class="form-check form-check-custom form-check-primary form-check-solid form-check-sm ms-10">
            <input class="form-check-input" type="radio" name="priority" formControlName="priority_n" value="N"
              (change)="showTextBox($event)" />
            <label class="form-check-label text-gray-700 text-nowrap" for="priorityNo">No</label>
          </div>
        </div>
        <br>
        <div *ngIf="showInput">
          <textarea type="text" rows="1"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            formControlName="priority_remarks" class="form-control" [(ngModel)]="inputValue"
            placeholder="Enter text here"></textarea>
        </div>
      </div>
    </div>

    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Remarks</b>
        <div class="col-12 col-lg-12 col-md-12 col-sm-12 fv-row mt-2">
          <textarea type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
            placeholder="" formControlName="remarks" class="form-control"></textarea>
        </div>
      </div>
    </div>
    <!-- end:: PO Add Form -->

    <!-- begin::Product Summary -->
    <div class="card shadow-sm">
      <!-- begin::Heading -->
      <div class="card-header bg-lightblue card-height55" style="border-radius: 0px;">
        <div class="card-title m-0">
          <h5 class="fw-bolder text-blue">Product Summary</h5>
        </div>
      </div>
      <!-- end::Heading -->
      <!-- begin::Product Form -->
      <form [formGroup]="productform" (ngSubmit)="productSubmit()">
        <div class="d-flex flex-row m-5 flex-wrap" style="border-radius: 0px;">
          <div class="col-lg-2 col-md-2 col-sm-2"><b>Product Name<span class="ms-1 text-red">*</span></b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <ng-select formControlName="productname" (change)="GetOnChangeProductName()" [class.is-invalid]="
                        product_name.invalid &&
                            (product_name.dirty || product_name.touched)
                          " [ngClass]="{'invalid-border': product_name.invalid && product_name.touched}"
                [items]="productdetails_list" required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                bindLabel="product_name"  placeholder="Select an option" [(ngModel)]="mdlProductName">
              </ng-select>
              <div *ngIf="
                        product_name.invalid &&
                            (product_name.dirty || product_name.touched)
                          " class="invalid-feedback">
                <div *ngIf="product_name.errors?.['required']">
                  Product Name is required.
                </div>
              </div>
          
            </div>
          </div>
       
          <div class="col-lg-1 col-md-1 col-sm-1"><b>Product Code</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control" formControlName="productcode" readonly style="background-color: #e9e7e7">
            </div>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1"><b>Unit</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control" formControlName="productuom_name" readonly style="background-color: #e9e7e7;">
            </div>
        </div>
        

       
        
        <div class="col-lg-2 col-md-2 col-sm-2"><b>Unit Price</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="number" class="form-control" placeholder="0.00" style="text-align: right;"
              formControlName="unitprice" [(ngModel)]="unitprice" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-lg-1 col-md-1 col-sm-1"><b>Quantity</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="number" class="form-control" style="text-align: right;" formControlName="quantity"
              [(ngModel)]="quantity" (input)="prodtotalcal()">
          </div>
        </div>

        <div class="col-lg-1 col-md-1 col-sm-1"><b>Discount(%)</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="number" class="form-control" placeholder="0.00" style="text-align: right;"
              formControlName="discountpercentage" [(ngModel)]="discountpercentage" (input)="prodtotalcal()">
          </div>

          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
            <input type="number" class="form-control" placeholder="0.00" readonly
              style="background-color: #e9e7e7; text-align: right;" formControlName="discountamount"
              [value]="discountamount" [(ngModel)]="discountamount" (input)="prodtotalcal()">
          </div>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1">
          <b>Tax </b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
              <ng-select formControlName="tax_name1" (change)="OnChangeTaxAmount1()"
                      [class.is-invalid]="product_code.invalid && (product_code.dirty || product_code.touched)"
                      [ngClass]="{'invalid-border': product_code.invalid && product_code.touched}" [items]="tax_list"
                      required class="custom col-lg-12 col-12 col-md-12 col-sm-12 fv-row mt-2" bindLabel="tax_name1"
                      bindValue="tax_gid" placeholder="Select an option" [(ngModel)]="mdlTaxName1">
              </ng-select>
          </div>
         
            <div class="col-lg-11 col-12 col-md-11 col-sm-8 fv-row mt-2">
              <input type="text" class="form-control" placeholder="0.00" readonly
                style="text-align: right; background-color: #e9e7e7;" formControlName="taxamount1"
                [(ngModel)]="taxamount1" (input)="OnChangeTaxAmount1()">
            </div>
        
      </div>
      
      <div class="col-lg-2 col-md-2 col-sm-2"><b>Total Amount</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="form-control" placeholder="0.00"
            style="background-color: #e9e7e7; text-align: right;" formControlName="totalamount"
            [(ngModel)]="totalamount" (input)="prodtotalcal()">
        </div>
      </div>


      <div class="col-lg-1 col-md-1 col-sm-1">
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
          <button type="button" class="btn-primary" style="margin-left: 20px;"
            (click)="productSubmit()">ADD</button>
        </div>
      </div>



        
       
     

        

         
        </div>
       
       
      </form>
      <!-- end::Product Form -->
      <!-- begin::table -->
      <div class="table-responsive">
        <div class="card card-body  m-0 shadow-lg p-3 mb-5 bg-white rounded">
          <table id="POproductlist" class="table table-striped table-row-bordered gy-4 gs-2">
            <thead class="table_head mb-4">
              <tr class="text-white fw-bold fs-5 text-nowrap  bg-lightblue card-height55 text-blue">
                <th>S.No</th>
                <th>Product Name</th>
                <th>Product Group</th>
                <th>Product Code</th>
                <!-- <th>HSN Code / Description</th> -->
                <th>Unit</th>
                <th style="text-align: right;">Unit Price</th>
                <th style="text-align: center;">Quantity</th>
                <th style="text-align: right;">Discount (%)</th>
                <th style="text-align: right; width: 120px;">Tax</th>
                <th style="text-align: right;">Total Amount</th>
                <th class="text-center">Action</th>
              </tr>
            </thead>
            <tbody>
              <tr *ngFor="let data of POproductlist; let i=index;">
                <td>{{i+1}}</td>
                <td>{{data.product_name}}</td>
                <td>{{data.productgroup_name}}</td>
                <td>{{data.product_code}}</td>
                <!-- <td>{{data.hsn}}</td> -->
                <td>{{data.productuom_name}}</td>
                <td style="text-align: right;">{{data.unitprice | number}}</td>
                <td style="text-align: center;">{{data.qty}}</td>
                <td style="text-align: right;">{{data.discount_percentage}}</td>
                <td style="text-align: right;">{{data.taxamount1}}</td>
                <td style="text-align: right;">{{data.product_total}}</td>
                <td class="text-center">
                  <i class="bi bi-trash-fill bg-danger text-white rounded p-2 m-2 fs-9"
                    data-bs-toggle="modal" data-bs-target="#myModaldeleteQA" (click)="openModaldelete(data.tmppurchaseorderdtl_gid)"></i>
                </td>
                
              </tr>
            </tbody>
          </table>
        </div>
        <div class="modal" id="myModaldeleteQA" data-bs-backdrop="static" data-bs-keyboard="false">
          <div class="modal-dialog del_modal-dialog">
            <div class="modal-content del_modal-content">
              <div class="text-center my-4">
                <h2><b>Are you sure?</b></h2>
                <p style="display: block;">Do You Want To Delete the Record ?</p>
                <div class="text-center my-4">
                  <button class="btn-danger btn-sm text-white me-4" data-bs-dismiss="modal" (click)="ondelete()">
                    <i class="bi bi-trash-fill text-white fs-5 me-2"></i>Delete
                  </button>
                  <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal">
                    <i class="fas fa-close text-white me-2"></i>Close
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- end::table -->
   

      <div class="card m-3">
    
       
              <div class="d-flex row justify-content-end mt-2">
                <div class="col-md-3 d-flex align-items-center">
                  <label class="col-sm-6 col-form-label fw-bolder" style="text-align:left;">Net Amount:</label>
                  <input type="text" formControlName="totalamount" class="form-control" style="width:100px" placeholder="0.00" style="width:100px;background-color: #e9e7e7; text-align: right;" readonly
            [(ngModel)]="netamount" (input)="finaltotal()">

              <span class="tbl_spn ms-4" >{{this.currency_code1}}</span>
                </div>
              </div>

              <div class="d-flex row justify-content-end mt-2">
                <div class="col-md-3 d-flex align-items-center">
                  <label class="col-sm-6 col-form-label fw-bolder">Tax:</label>
                  <ng-select class=" custom
                        col-lg-10 col-12
                        fv-row
                        mt-2" style="width:70px" [items]="tax_list" formControlName="tax_name" bindLabel="tax_name"
                    bindValue="tax_gid" placeholder="Select Tax" [(ngModel)]="mdlTaxName4" (change)="OnChangeTaxAmount4()">
                  </ng-select>&nbsp;
                  <input type="text" class="form-control" style="width:70px" (input)="OnChangeTaxAmount4()" readonly
                    style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount4"
                    [(ngModel)]="tax_amount4">&nbsp;
                  <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
                </div>
              </div>

        <div class="d-flex row justify-content-end mt-2">
          <div class="col-md-3 d-flex align-items-center">
            <label class="col-sm-6 col-form-label fw-bolder">Addon Charges:</label>
