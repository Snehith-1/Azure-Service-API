<div class="card shadow-sm">
  <div class="card card-body">
    <div class="card-title m-0">
      <h3 class="fw-bolder text-blue">Amend Sales Order</h3>
    </div>
    <br>
    <input type="hidden" formControlName="tmpsalesorderdtl_gid">
    <!-- (ngSubmit)="onSubmit()" -->
    <form [formGroup]="combinedFormData">
      <div class="d-flex flex-row m-6 flex-wrap">
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Ref. No</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="New Purchase Order" readonly formControlName="salesorder_gid" class="form-control" />
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Date</b><span class="ms-1 text-red">*</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="date"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              formControlName="salesorder_date"  class="form-control" [(ngModel)]="salesorder_date" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch </b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
               readonly formControlName="branch_name" class="form-control" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Ref. No</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="Customer Reference Number" formControlName="so_referencenumber" class="form-control" />
          </div>
        </div>
      </div>
      <div class="d-flex flex-row m-6 flex-wrap">
        <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Customer</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input  custom
              col-lg-11 col-12
              fv-row
              mt-2"
              placeholder="Customer Name" formControlName="customer_name" readonly class="form-control" />
        </div>
      </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Person<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              readonly formControlName="customercontact_names" class="form-control" style="background-color: #e9e7e7" />
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Number</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              readonly formControlName="customer_mobile" class="form-control" style="background-color: #e9e7e7" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Email Address</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              formControlName="customer_email" class="form-control" style="background-color: #e9e7e7" />
          </div>
        </div>
      </div>

      <div class="d-flex flex-row m-6 flex-wrap">
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Address</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <textarea type="text" rows="1"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              style="background-color: #e9e7e7" readonly formControlName="customer_address"
              class="form-control"></textarea>
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Remarks</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <textarea type="text" rows="1"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="Enter Remarks" formControlName="so_remarks" class="form-control"></textarea>
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping Address</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <textarea type="text" rows="1"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Shipping Address" formControlName="shipping_to" class="form-control"></textarea>
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Sales Person<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <ng-select [items]="contact_list" class="
          custom
          col-lg-11 col-12
          fv-row
          mt-2
        " [ngStyle]="{
          'border-color':
          user_name.invalid && user_name.touched
              ? 'red'
              : 'rgb(209, 209, 209)'
        }" bindLabel="user_name" formControlName="user_name" bindValue="user_gid" placeholder="Select an option"
              [(ngModel)]="mdlUserName"> </ng-select>
            <div *ngIf="user_name.invalid && (user_name.dirty || user_name.touched)">
              <div *ngIf="user_name.errors?.['required']">
                <span style="color:red;">Select Sales Person.</span>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="d-flex flex-row m-6 flex-wrap">
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Expected Start Date</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="date"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              formControlName="start_date" class="form-control" />
          </div>
        </div>


        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Esimated Time of Arraival</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="date"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              formControlName="end_date" class="form-control" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Freight terms" formControlName="freight_terms" class="form-control" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Payment Terms" formControlName="payment_terms" class="form-control" />
          </div>
        </div>
      </div>

      <div class="d-flex flex-row m-6 flex-wrap">
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">

            <ng-select formControlName="currency_code" [items]="currency_list" (change)="OnChangeCurrency()"
              (change)="onCurrencyCodeChange($event)" required
              class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="currency_code"
              bindValue="currencyexchange_gid" placeholder="Select an option" [(ngModel)]="mdlCurrencyName">
              <option *ngFor="let currency of currency_list" [value]="currency.currencyexchange_gid">
                {{currency.currency_code}}</option>
            </ng-select>
            <div *ngIf="currency_code.invalid && (currency_code.dirty || currency_code.touched)">
              <div *ngIf="currency_code.errors?.['required']">
                <span style="color:red;">Select Currency.</span>
              </div>
            </div>
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchane Rate</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Exchange Rate" formControlName="exchange_rate" class="form-control" (keypress)="onKeyPress($event)" />
          </div>
        </div>
      </div>


      <!-- end:: PO Add Form -->

      <!-- begin::Product Summary -->


      <!-- begin::Heading -->
      <div class="card-header bg-lightblue card-height55" style="border-radius: 0px;">
        <div class="card-title m-0">
          <h5 class="fw-bolder text-blue">Product Summary</h5>
        </div>
      </div>

      <form [formGroup]="productform" (ngSubmit)="productAdd()">
        <div class="d-flex flex-row flex-wrap m-6">
          <div class="col-12 col-lg-2 col-md-2 col-sm-12"><b>Product<span class="ms-1 text-red">*</span></b>
            <ng-select [items]="product_list"
            class="custom col-lg-11 col-12 fv-row mt-2" (change)="GetOnChangeProductsName()"
             bindLabel="product_name"  formControlName="product_name" class="fs-8"
             placeholder="Select an option" [(ngModel)]="mdlproductName"></ng-select>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Product Code</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control" formControlName="product_code"
               readonly style="background-color: #e9e7e7;">
            </div>
          </div>
 
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Unit</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control" style="text-align: right;" 
              formControlName="unit" readonly>
            </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Unit Price</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control" placeholder="0.00"  (input)="prodtotalcal()"
                    style="text-align: right;" formControlName="unitprice" [(ngModel)]="unitprice"
                    (keypress)="onKeyPress($event)">
            </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Quantity</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control" style="text-align: right;"
              (input)="prodtotalcal()" formControlName="quantity" [(ngModel)]="quantity"
              (keypress)="onKeyPress($event)">
             </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Discount(%)</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control" placeholder="0.00" style="text-align: right;"
               (input)="prodtotalcal()" formControlName="discountpercentage" [(ngModel)]="discountpercentage"
                (keypress)="onKeyPress($event)">
            </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b></b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
              <input type="text" class="form-control mt-2"
              (input)="prodtotalcal()" placeholder="0.00" 
              readonly style="background-color: #e9e7e7; text-align: right;"
               formControlName="discountamount" [(ngModel)]="discountamount">                   
             </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Tax<span class="ms-1 text-red">*</span></b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2 ">
              <ng-select [items]="tax_list" bindLabel="tax_name" formControlName="tax_name"
              (change)="OnChangeTaxAmount1()" class="fs-8"
              bindValue="tax_gid" placeholder="Select Tax" [(ngModel)]="mdlTaxName"></ng-select>
            </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b></b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
              <input type="text" class="form-control mt-2" placeholder="0.00" 
              readonly style="text-align: right; background-color: #e9e7e7;"
               formControlName="tax_amount" [(ngModel)]="tax_amount" (change)="OnChangeTaxAmount1()">
            </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12"><b>Total Amount</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" class="form-control" placeholder="0.00" 
              (input)="prodtotalcal()" style="background-color: #e9e7e7; text-align: right;"
              readonly formControlName="totalamount" [(ngModel)]="totalamount">
            </div>
          </div>
 
          <div class="col-12 col-lg-1 col-md-1 col-sm-12">
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
              <div class="text-center text-nowrap">
                <button title="Add" type="button"
                (click)="productAdd()"
                  class="btn btn-icon bg-success btn-sm  me-2">+</button>
                  
          <button title="Update" type="button" 
          (click)="productUpdate()"
          class="btn btn-icon btn-sm bg-success me-2" ><span
            class="fa-solid fa-arrow-up text-white  fs-6"></span></button>
              </div>
            </div>
          </div>
        </div>
        <div class="table-responsive">
          <div class="card card-body table-responsive m-0 shadow-lg p-3 mb-5 bg-white rounded">
            <table id="salesorders_list" class="table table-striped table-row-bordered gy-4 gs-2">
              <thead class="table_head">
                <tr class="text-white fw-bold fs-7 text-nowrap bg-lightblue text-blue">
                  <th>S.No</th>
                  <th>Product </th>
                  <th>Product Group</th>
                  <th class="ml-05">Product Code</th>
                  <!-- <th>HSN Code / Description</th> -->
                  <th>Unit</th>
                  <th class="ml-05" style="text-align: right;">Unit Price</th>
                  <th style="text-align: center;">Quantity</th>
                  <th style="text-align: right;">Discount (%)</th>
                  <th style="text-align: right;">Tax</th>
                  <th style="text-align: right;">Total Amount</th>
                  <th class="text-center">Action</th>
                </tr>
              </thead>
              <tbody>
                <tr *ngFor="let data of salesorders_list; let i=index;">
                  <td>{{i+1}}</td>
                  <td>{{data.product_name}}</td>
                  <td>{{data.productgroup_name}}</td>
                  <td>{{data.product_code}}</td>                 
                  <td style="text-align: right;">{{data.uom_name}}</td>
                  <td>{{data.product_price}}</td>
                  <td style="text-align: center;">{{data.qty_quoted}}</td>
                  <td style="text-align: right;">
                    <div class="container">
                      <div class="row">
                        {{data.discount_percentage}}&nbsp;
                      </div>
                      <div class="row">
                        {{data.discount_amount}}
                      </div>
                    </div>
                  </td>
                 
                  <td style="text-align: right;">
                    <div class="row">
                      <div class="col">
                        {{data.tax_name}}&nbsp;
                        {{data.tax_amount}}
                      </div>
                    </div>
                    
                  </td>
                  <td>
                    {{data.totalamount}}
                  </td>

                  <td class="text-center text-nowrap">
                    <button title="Delete" type="button" data-bs-toggle="modal" data-bs-target="#myModaldeleteSO"
                      class="btn btn-icon btn-sm bg-danger me-2"
                      (click)="openModaldelete(data.tmpsalesorderdtl_gid)"><span
                        class="fa-solid fa-trash text-white  fs-6"></span></button>
                      
              <button title="Edit" type="button" class="btn btn-icon btn-sm bg-success me-2" 
              (click)="edit(data)" ><span
                class="fa-solid fa-pencil text-white  fs-6"></span></button>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
          <div class="modal" id="myModaldeleteSO" data-bs-backdrop="static" data-bs-keyboard="false">
            <div class="modal-dialog del_modal-dialog">
              <div class="modal-content del_modal-content">
                <div class="text-center my-4">
                  <h2><b>Are you sure?</b></h2>
                  <p style="display: block;">Do You Want To Delete the Record ?</p>
                  <div class="text-center my-4">
                    <button class="btn-danger btn-sm text-white me-4" data-bs-dismiss="modal" (click)="ondelete()">
                      <i class="bi bi-trash-fill text-white fs-5 me-2"></i>Delete
                    </button>
                    <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal">
                      <i class="fas fa-close text-white me-2"></i>Close
                    </button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </form>


      <!-- end::table -->
      <!-- begin::calculation part -->
      <div class="card">

        <div class="d-flex flex-row m-6 flex-wrap">
          <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Payment Days</b><span class="ms-1 text-red">*</span>
            <div class="col-lg-8 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
                formControlName="payment_days" class="form-control" placeholder="Payment Days" (keypress)="onKeyPress($event)"/>

            </div>
          </div>

          <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Delivery Days</b> <span class="ms-1 text-red">*</span>
            <div class="col-lg-8 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text" placeholder="Delivery Days"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
                formControlName="delivery_days" class="form-control" (keypress)="onKeyPress($event)" />
            </div>
          </div>
        </div>

        <div class="card">
          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Net Amount:</label>
              <input type="text" class="form-control" formControlName="producttotalamount"
                [(ngModel)]="producttotalamount" style="width:100px;background-color: #e9e7e7; text-align: right;"
                readonly (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
              <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
            </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Addon Charges:</label>
              
              <input type="text" formControlName="addon_charge" class="form-control" style="width:100px"
                placeholder="0.00" [(ngModel)]="addon_charge" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
                <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
            </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Additional Discount:</label>
              <!-- <input type="text" class="form-control-plaintext" readonly style="width:70px"  placeholder="0.00">&nbsp; -->
              
              <input type="text" formControlName="additional_discount" class="form-control" style="width:100px"
                placeholder="0.00" [(ngModel)]="additional_discount" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
                <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
              </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Freight Charges:</label>
              <input type="text" formControlName="freight_charges" class="form-control" style="width:100px"
                placeholder="0.00" [(ngModel)]="freight_charges" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
                <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
              </div>
          </div>
          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Buy Back/Scrap:</label>
              <!-- <input type="text" class="form-control-plaintext" readonly style="width:70px"  placeholder="0.00">&nbsp; -->
             
              <input type="text" formControlName="buyback_charges" class="form-control" style="width:100px"
                placeholder="0.00" [(ngModel)]="buyback_charges" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
                <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
               </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Packing/Forwarding Charges:</label>
              <input type="text" class="form-control" style="width:100px" formControlName="packing_charges"
                placeholder="0.00" [(ngModel)]="packing_charges" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">&nbsp;
              <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
            </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Insurance Charges:</label>                       
              <input type="text" formControlName="insurance_charges" class="form-control" style="width:100px"
                placeholder="0.00" [(ngModel)]="insurance_charges" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
                <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
              </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Round Off:</label>
              <!-- <input type="text" class="form-control" readonly style="width:70px"  placeholder="0.00">&nbsp; -->
              
              <input type="text" formControlName="roundoff" class="form-control" style="width:100px" placeholder="0.00"
                [(ngModel)]="roundoff" (ngModelChange)="finaltotal()" (keypress)="onKeyPress($event)">
              <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>
              </div>
          </div>

          <div class="d-flex row justify-content-end mt-2">
            <div class="col-md-3 d-flex align-items-center">
              <label class="col-sm-6 col-form-label fw-bolder">Grand Total:</label>
              <!-- <input type="text" class="form-control" readonly style="width:70px"  placeholder="0.00">&nbsp; -->
             
              <input type="text" formControlName="grandtotal" readonly class="form-control"
                style="width:100px;background-color: #e9e7e7; text-align: right;" placeholder="0.00"
                [(ngModel)]="grandtotal" >
            <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{this.currency_code1}}</span>

              </div>
          </div>
          <!--Summary count ends-->

          <div class="card-header bg-lightblue card-height55">
            <div class="card-title m-0">
              <h5 class="fw-bolder text-blue">Terms and Conditions</h5>
            </div>
          </div>
          <ng-select [items]="terms_list" class="custom col-lg-5 m-12 col-12 fv-row mt-2" bindLabel="template_name"
            [(ngModel)]="mdlTerms" formControlName="template_name" bindValue="template_gid"
            placeholder="Select an option" (change)="GetOnChangeTerms()"></ng-select>
          <div class="d-flex flex-row m-12 flex-wrap table-responsive">
            <div class="col-12 col-lg-12 col-md-10  col-sm-10 fv-row mt-2">
              <angular-editor [config]="config" formControlName="termsandconditions"></angular-editor>
            </div>
          </div>


          <div class="text-center my-4">
            <button type="submit" class="btn-success btn-sm text-white me-4"
            (click)="overallsubmit()" data-bs-dismiss="modal">
              <i class="fas fa-check text-white fs-5 "></i> &nbsp;Amend
            </button> &nbsp;&nbsp;
            <button class="btn-danger btn-sm text-white me-4" (click)="close()" data-bs-dismiss="modal">
              <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
            </button>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>