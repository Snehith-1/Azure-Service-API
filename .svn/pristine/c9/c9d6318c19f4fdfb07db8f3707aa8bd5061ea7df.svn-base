<!-- begin::Add PO -->
<div class="card ">
    <!-- begin::Heading -->
    <div class="card-header bg-primary card-height55">
        <div class="card-title m-0">
            <h3 class="fw-bolder text-white"><i class="fas fa-eye fw-bolder fs-3"></i>&nbsp;&nbsp;View Purchase Order</h3>
        </div>
    </div>
    <!-- end::Heading -->

    <!-- begin:: PO Add Form-->

<div>
    <div class="d-flex flex-row mt-6 flex-wrap border-bottom ms-7">
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Ref.No</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <p>{{this.purchaseorder_gid}}</p>
            </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Date</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <p>{{this.purchaseorder_date}}</p>
            </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch Name</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <p>{{this.branch_name}}</p>

            </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO No</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <p>{{this.po_no}}</p>

            </div>
        </div>

</div>
</div>

    <div >
        <div class="d-flex flex-row mt-6 flex-wrap border-bottom ms-7">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Despatch To Branch</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.deliverytobranch}}</p>

                </div>
            </div>
           
            <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Priority</b>
                <div class="d-flex justify-content-start mt-4 ms-2" >
                    <p>{{this.priority_n}}</p>
                </div>
            </div>
           
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Name</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.vendor_companyname}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact Person</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.vendor_contact_person}}</p>

                </div>
            </div> 
          

          
        </div>
    </div>


    <div>
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom ms-7">
           
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Mobile</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.vendor_contactnumber}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Email </b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.vendor_emailid}}</p>

                </div>
            </div>   

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Address</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.vendor_address}}</p>

                </div>
            </div>  



            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Fax</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.vendor_faxnumber}}</p>
                    <!-- <input type="text"
                        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                        placeholder="Vendor Fax"  formControlName="vendor_fax" class="form-control" /> -->
                </div>
            </div>

           
          
        </div>
    </div>


    <div>
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom ms-7">

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Ship Via</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.ship_via}}</p>

                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping Address</b>
                <div class="col-lg-11 col-12 col-md-12 col-sm-12 fv-row mt-2">
                    <p>{{this.shipping_address}}</p>

                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Delivery Location</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.delivery_location}}</p>

                </div>
            </div>

        
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Cover Note</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.purchaseorder_reference}}</p>

                </div>
            </div>
       
          

        </div>

    </div>


    <div>
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom ms-7">
          
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.freight_terms}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.payment_terms}}</p>

                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.currency_code}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchange Rate</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{this.exchange_rate}}</p>

                </div>
            </div>

          
        </div>
    </div>
    <div class="d-flex flex-row m-6 flex-wrap ms-7">
        <div>
            <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Remarks</b>
                <div class="col-12 col-lg-12 col-md-12 col-sm-12 fv-row mt-2">
                    <p>{{this.purchaseorder_remarks}}</p>

                </div>
            </div>
        </div>
    </div>


<!-- end:: PO Add Form -->


<!-- begin::table -->

<div class="table-responsive ms-4">
    <div >
        <table id="employee_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
            <thead>
                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                    <th>Product Group</th>
                    <th>Product Code</th>
                    <th>Product Name</th>
                    <th>Unit</th>
                    <th>Qty Ordered</th>
                    <th>Qty Recieved</th>
                    <th>Qty Adjusted</th>
                    <th>Unit Price</th>
                    <th>Discount(%)</th>
                    <th>Tax</th>
                    <th>Total Amount</th>
                    
                </tr>
            </thead>
            <tbody *ngFor="let data of purchaseorder_list;">
                <tr>
                    <td>{{data.productgroup_name}}</td>
                    <td>{{data.product_code}}</td>
                    <td>{{data.product_name}}</td>
                    <td>{{data.productuom_name}}</td>
                    <td>{{data.qty_ordered}}</td>
                    <td>{{data.qty_Received}}</td>
                    <td>{{data.qty_grnadjusted}}</td>
                    <td>{{data.product_price}}</td>
                    <td>{{data.discount_percentage}}</td>
                    <td>{{data.tax_name}}&nbsp;&nbsp;: {{data.tax_amount}}
                  
                    </td>
                    <td>{{data.product_total}}</td>
                    
                </tr>
            </tbody>
        </table>
    </div>
</div>
</div>
<!-- end::table -->
<!-- end::Product Summary -->


<div class="card">
    <div class="d-flex flex-row m-6 mb-0 flex-wrap">
      <div class="col-12 col-lg-3 col-md-6 col-sm-12 ms-3 "><span class="fw-bold">Payment Days</span>
        <p class="mt-3">{{this.payment_days}}</p>
       
      </div>
 
      <div class="col-12 col-lg-2 col-md-4 col-sm-12"><span class="fw-bold">Delivery Days</span>
        <p class="mt-3">{{this.delivery_days}}</p>
       
      </div>
    </div>
  </div>

<div class="card">

    <div class="me-10">

        <div class="d-flex justify-content-end mt-4">
            
            <div class="col-md-3 d-flex justify-content-between mt-4 ">
                <label class="tbl_lab ">Net Amount:</label>
                <span class="tbl_spn ms-12">{{this.total_amount}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
            </div>
        </div>

        <div class=" d-flex justify-content-end mt-4">
            <div class="col-md-3 d-flex justify-content-between mt-4">
                <label class="tbl_lab">Tax :</label>
                <span class="tbl_spn">{{this.overall_tax}}</span>&nbsp;&nbsp;<span class="tbl_spn ms-12">{{this.currency_code}}</span>
            </div>
        </div>

        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-3 d-flex justify-content-between mt-4">
                <label class="tbl_lab">Add On Charges :</label>
                <span class="tbl_spn">{{this.addon_amount}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-3 d-flex justify-content-between mt-4">
                <label class="tbl_lab">Additional Discount :</label>
                <span class="tbl_spn">{{this.discount_amount}}</span>
                <span class="tbl_spn ms-12">{{this.currency_code}}</span>
            </div>
        </div>
        <div >
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Freight Charges :</label>

                    <span class="tbl_spn">{{this.freighttax_amount}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div>
            <!-- <div class="d-flex justify-content-end mt-4 ">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab"> Freight Tax :</label>
                    <span class="tbl_spn">{{this.freighttax_amount}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div> -->

            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Buy Back/Scrap :</label>
                    <span class="tbl_spn">{{this.buybackorscrap}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Packing Charges :</label>
                    <span class="tbl_spn">{{this.packing_charges}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Insurance Charges :</label>
                    <span class="tbl_spn">{{this.insurance_charges}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Round Off :</label>
                    <span class="tbl_spn">{{this.roundoff}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div>

            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Grand Total :</label>
                    <span class="tbl_spn">{{this.total_amount}}</span><span class="tbl_spn ms-12">{{this.currency_code}}</span>
                </div>
            </div>

        </div>
    </div>
<div class="card">
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">Terms and Conditions</h5>
        </div>
    </div>
    <br>
    <div>
            <div class="col-12 col-lg-12 col-md-10  col-sm-10 fv-row mt-2">
                <p>{{this.termsandconditions}}</p>

            </div>
        </div>
        <!-- <div class="d-add flex-row m-2 flex-wrap table-responsive">
            <div class="col-12 col-lg-12 col-md-10  col-sm-10 fv-row mt-2">
                <angular-editor [config]="config"
                    formControlName="template_content">{{data.termsandconditions}}</angular-editor>
            </div>
        </div> -->
    </div>
</div>

