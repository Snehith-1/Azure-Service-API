
<div class="card">
    <div class="row ms-3 mt-3 d-flex align-items-center">
        <div class="col">
          <h3 class="card-title">Delivery Order</h3>
        </div>
        
      </div> 
    <div class="table-responsive">
        <div class="mx-3 p-3 pt-0 pb-0 bg-white rounded">
          <table id="deliveryadd_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
            <thead>
              <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                <th>S.NO </th>
                <th>DO Date</th>
                <th>DO Reference No</th>
                <th>Customer</th>
                <th>Contact Details </th>
                <th>Details </th>
                <th>Status</th>
                <th>Action </th>
                
              </tr>
            </thead>
            <tbody>
              <tr *ngFor="let data of deliveryadd_list; let i=index;">
                <td>{{i+1}}</td>
                <td>{{data.directorder_date}}</td>
                <td>{{data.directorder_refno}}</td> 
                <td>{{data.customer_name}}</td>
                <td>{{data.customer_contactperson}}</td>  
                <td>{{data.delivery_details}}</td>       
                <td>{{data.delivery_status}}</td>
                <td>
                    <button title="Delivery Order" type="button" 
                  class="btn btn-icon btn-sm bg-warning me-2" (click)="onadd(data.directorder_gid)" routerLink="/ims/ImsTrnDeliveryacknowledgementUpdate" >
                  <span class="fa-solid fa-book text-white  fs-6"></span></button> 
                </td>
                
    
           
                
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    
