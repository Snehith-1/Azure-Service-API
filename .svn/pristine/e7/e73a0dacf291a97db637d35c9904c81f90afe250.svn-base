                          <input formControlName="deg_dip" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': deg_dip.invalid && deg_dip.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Degree / Diploma" pattern="^(?!\s*$).+" name="deg_dip" required />
                          <div
                            *ngIf="reactiveForm4.get('deg_dip')?.hasError('required') && reactiveForm4.get('deg_dip')?.touched || reactiveForm4.get('deg_dip')?.hasError('pattern')"
                            class="text-red">Degree / Diploma is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Field of
                          Study</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="field_ofstudy" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': field_ofstudy.invalid && field_ofstudy.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Field of Study" pattern="^(?!\s*$).+" name="field_ofstudy" required />
                          <div
                            *ngIf="reactiveForm4.get('field_ofstudy')?.hasError('required') && reactiveForm4.get('field_ofstudy')?.touched || reactiveForm4.get('field_ofstudy')?.hasError('pattern')"
                            class="text-red">Field of Study is required.</div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Date of
                          completion</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="date_ofcompletion" type="date"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': date_ofcompletion.invalid && date_ofcompletion.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Date of completion" pattern="^(?!\s*$).+" name="date_ofcompletion"
                            required />
                          <div
                            *ngIf="reactiveForm4.get('date_ofcompletion')?.hasError('required') && reactiveForm4.get('date_ofcompletion')?.touched || reactiveForm4.get('date_ofcompletion')?.hasError('pattern')"
                            class="text-red">Date of completion is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Additional Notes</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <textarea
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 border-secondary bg-white custom-input"
                            formControlName="cont_addr" rows="1" cols="18" placeholder="Enter Additional Notes"
                            name="cont_addr"></textarea>
                        </div>
                      </div>
                    </div>

                    <div class="text-center my-4 mt-10">
                      <button class="btn-success btn-sm text-white me-2" (click)="educationsubmit()"
                        [disabled]="reactiveForm4.invalid">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
                      </button>
                    </div>

                    <div class="table-responsive">
                      <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
                        <table id="department_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
                          <thead>
                            <tr class="fw-bold fs-5 bg-lightblue text-blue">
                              <th>S.No</th>
                              <th>Institution Name</th>
                              <th>Degree / Diploma</th>
                              <th>Field of Study</th>
                              <th>Date of Completion</th>
                              <th>Additional Notes</th>
                            </tr>
                          </thead>
                          <tbody>
                            <tr *ngFor="let data of educationdtllist; let i=index;">
                              <td>{{i+1}}</td>
                              <td>{{data.institution_name}}</td>
                              <td>{{data.degree_diploma}}</td>
                              <td>{{data.field_study}}</td>
                              <td>{{data.date_completion}}</td>
                              <td>{{data.address_notes}}</td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </form>
              </div>

              <!--Policy Management-->
              <div class="row tab-pane fade ms-5" id="kt_tab_pane_12" role="tabpanel">
                <div class="row" style="padding-right: 10px; padding-left: 10px;">
                  <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                    <h3 class="fw-bolder text-white"><i class="fa-solid fa-shield-halved fs-4 me-5"></i>Policy
                      Management
                    </h3>
                  </div>

                  <div class="table-responsive">
                    <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
                      <table class="table table-striped table-row-bordered gy-5 gs-7">
                        <tbody>
                          <tr *ngFor="let policy of policies_list">
                            <td>
                              <div class="card shadow-sm" style="width: 200px; background-color: #2196f3;">
                                <div class="card-header text-right text-center mt-03">
                                  <div class="text-lightblue fw-bold mt-7 mr-2 ml-10" style="color: #ffffff;">
                                    {{ policy.policy_name }}
                                  </div>
                                </div>
                              </div>
                            </td>
                            <td>
                              <div class="card shadow-sm"
                                style="width: 300px; background-color: #b5daf8; border: 1px solid #2196f3;">
                                <div class="card-header">
                                  <div class="mt-4 mb-3" *ngFor="let point of policy.policy_desc.split('tab')">
                                    {{ point }}<br>
                                  </div>
                                </div>
                              </div>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>