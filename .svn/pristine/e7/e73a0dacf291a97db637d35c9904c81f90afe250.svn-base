<div class="card shadow-lg rounded-top">
  <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
    <h3 class="fw-bolder text-white">
      <i class="fa fa-user fw-bolder fs-3 ms-5 me-5"></i>My Profile
    </h3>
  </div>

  <div class="back-icon">
    <i class="bi bi-arrow-left-square-fill" (click)="back()"></i>
  </div>

  <div class="card">
    <div class="card-body">
      <div class="row">
        <div class="col-lg-3 col-md-12 col-sm-12">
          <div class="card shadow-sm p-3 ">
            <div class="col-lg-12 col-md-6 col-sm-12 text-center symbol symbol-150px symbol-circle">
              <img src="assets\media\images\images.png" width="120%" height="200">
            </div>
            <div class="text-center mt-6">
              <label for="">{{emp_code}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_name}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_designation}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_department}}</label>
            </div>
          </div>
        </div>

        <div class="col-lg-9 col-md-12 col-sm-12">
          <ul class="nav nav-tabs nav-line-tabs nav-line-tabs-20x mb-8 fs-6">
            <li class="nav-item">
              <a class="nav-link active" data-bs-toggle="tab" href="#kt_tab_pane_4"><b>Personal Details</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_5"><b>Change Password</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_6"><b>Work Experience</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7"><b>Nomination</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_8"><b>Statutory</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_9"><b>Emergency Contact</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_10"><b>Dependent</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_11"><b>Education</b></a>
            </li>

            <li class="nav-item ms-9">
              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_12"><b>Policy</b></a>
            </li>
          </ul>

          <div class="card shadow-sm p-3">
            <div class="tab-content" id="myTabContent">
              <!-- Personal details -->
              <div class="tab-pane fade show active ms-5" id="kt_tab_pane_4" role="tabpanel">
                <form [formGroup]="reactiveForm7Edit" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa fa-info-circle fs-4 me-5"></i>Personal Details</h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">First Name</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="firstedit_name" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': firstedit_name.invalid && firstedit_name.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter First Name" pattern="^(?!\s*$).+" name="firstedit_name" required />
                          <div
                            *ngIf="reactiveForm7Edit.get('firstedit_name')?.hasError('required') && reactiveForm7Edit.get('firstedit_name')?.touched || reactiveForm7Edit.get('firstedit_name')?.hasError('pattern')"
                            class="text-red">First name is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Last Name</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="lastedit_name" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': lastedit_name.invalid && lastedit_name.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Last Name" pattern="^(?!\s*$).+" name="lastedit_name" required />
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Date of Birth</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="dateof_birthedit" type="date"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': dateof_birthedit.invalid && dateof_birthedit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter First Name" pattern="^(?!\s*$).+" name="dateof_birthedit" required />
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">

                      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><span class="font-bold required">Blood Group</span>
                        <ng-select
                          [ngClass]="{'invalid-border': reactiveForm7Edit.get('blood_groupedit')?.invalid && reactiveForm7Edit.get('blood_groupedit')?.touched}"
                          [items]="bloodgroup_list" required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                          bindLabel="bloodgroup_name" placeholder="Select an option" formControlName="blood_groupedit"></ng-select>
                        <div
                          *ngIf="reactiveForm7Edit.get('blood_groupedit')?.hasError('required') && reactiveForm7Edit.get('blood_groupedit')?.touched"
                          class="text-red">
                          Blood Group is required.
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Gender</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-6">
                          <input class="form-check-input" formControlName="gender_edit" type="radio" value="male"
                            value="MALE" />
                          <label for="html" class="ms-3"><b>Male</b></label>
                          <input class="form-check-input ms-10" formControlName="gender_edit" type="radio"
                            value="female" value="FEMALE" />
                          <label for="html" class="ms-3"><b>Female</b></label>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Mobile Number</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="mobile_edit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': mobile_edit.invalid && mobile_edit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Mobile Number" name="mobile_edit" maxlength="10" required />
                        </div>
                        <div *ngIf="mobile_edit.invalid && (mobile_edit.dirty || mobile_edit.touched)">
                          <div *ngIf="mobile_edit.errors?.['pattern']">
                            <span
                              style="color: red; width: 200px; margin-top: 5px; margin-left: 8px; font-size: 11px;">*
                              Maximum 10 digits</span>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Personal Number</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="personal_noedit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': personal_noedit.invalid && personal_noedit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Personal Number" name="personal_noedit" maxlength="10" required />
                        </div>
                        <div *ngIf="personal_noedit.invalid && (personal_noedit.dirty || personal_noedit.touched)">
                          <div *ngIf="personal_noedit.errors?.['pattern']">
                            <span
                              style="color: red; width: 200px; margin-top: 5px; margin-left: 8px; font-size: 11px;">*
                              Maximum 10 digits</span>
                          </div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Qualification</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="qualification_edit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': qualification_edit.invalid && qualification_edit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Qualification" pattern="^(?!\s*$).+" name="qualification_edit"
                            required />
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Experience</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="experience_edit" type="text"
                            class="form-control form-control-lg form-control-solid borderc-secondary custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': experience_edit.invalid && experience_edit.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Experience" pattern="^(?!\s*$).+" name="experience_edit" required />
                        </div>
                      </div>
                    </div>
                    <div class="text-center my-4 mt-10">
                      <button class="btn-success btn-sm text-white me-2" (click)="detailsupdate()"
                        [disabled]="reactiveForm7Edit.invalid">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Update
                      </button>
                    </div>
                  </div>
                </form>
              </div>

              <!--Change Password-->
              <div class="row tab-pane fade ms-5" id="kt_tab_pane_5" role="tabpanel">
                <form [formGroup]="reactiveForm8Edit" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa fa-key fs-4 me-5"></i>Change Password
                      </h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Current
                          Password</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="curredit_pwd" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': curredit_pwd.invalid && curredit_pwd.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Current Password" pattern="^(?!\s*$).+" name="curredit_pwd" required />
                          <div
                            *ngIf="reactiveForm8Edit.get('curredit_pwd')?.hasError('required') && reactiveForm8Edit.get('curredit_pwd')?.touched || reactiveForm8Edit.get('curredit_pwd')?.hasError('pattern')"
                            class="text-red">Current Password is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">New
                          Password</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="newedit_pwd" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': newedit_pwd.invalid && newedit_pwd.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter New Password" pattern="^(?!\s*$).+" name="newedit_pwd" required />
                          <div
                            *ngIf="reactiveForm8Edit.get('newedit_pwd')?.hasError('required') && reactiveForm8Edit.get('newedit_pwd')?.touched || reactiveForm8Edit.get('newedit_pwd')?.hasError('pattern')"
                            class="text-red">New Password is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Confirm
                          Password</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="confedit_pwd" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': confedit_pwd.invalid && confedit_pwd.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Confirm Password" pattern="^(?!\s*$).+" name="confedit_pwd" required />
                          <div
                            *ngIf="reactiveForm8Edit.get('confedit_pwd')?.hasError('required') && reactiveForm8Edit.get('confedit_pwd')?.touched || reactiveForm8Edit.get('confedit_pwd')?.hasError('pattern')"
                            class="text-red">Confirm Password is required.</div>
                        </div>
                      </div>
                    </div>

                    <div class="text-center my-4 mt-10">
                      <button class="btn-success btn-sm text-white me-2" (click)="passwordupdate()"
                        [disabled]="reactiveForm8Edit.invalid">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Update
                      </button>
                    </div>

                  </div>
                </form>
              </div>

              <!--Work Experience-->
              <div class="tab-pane fade ms-5" id="kt_tab_pane_6" role="tabpanel">
                <form [formGroup]="reactiveForm6" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa-solid fa-briefcase fs-4 me-5"></i>Work Experience
                      </h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Employee's
                          Previous Company</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="empl_prevcomp" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': empl_prevcomp.invalid && empl_prevcomp.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Previous company" pattern="^(?!\s*$).+" name="empl_prevcomp" required />
                          <div
                            *ngIf="reactiveForm6.get('empl_prevcomp')?.hasError('required') && reactiveForm6.get('empl_prevcomp')?.touched || reactiveForm6.get('empl_prevcomp')?.hasError('pattern')"
                            class="text-red">Previous company is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Employee
                          Code</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="empl_code" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': empl_code.invalid && empl_code.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Employee Code" pattern="^(?!\s*$).+" name="empl_code" required />
                          <div
                            *ngIf="reactiveForm6.get('empl_code')?.hasError('required') && reactiveForm6.get('empl_code')?.touched || reactiveForm6.get('empl_code')?.hasError('pattern')"
                            class="text-red">Employee Code is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Previous
                          Occupation</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="prev_occp" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': prev_occp.invalid && prev_occp.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Previous Occupation" pattern="^(?!\s*$).+" name="prev_occp" required />
                          <div
                            *ngIf="reactiveForm6.get('prev_occp')?.hasError('required') && reactiveForm6.get('prev_occp')?.touched || reactiveForm6.get('prev_occp')?.hasError('pattern')"
                            class="text-red">Previous Occupation is required.</div>
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Date of
                          Joining</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="date_ofjoining" type="date"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': date_ofjoining.invalid && date_ofjoining.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="" pattern="^(?!\s*$).+" name="date_ofjoining" />
                          <div
                            *ngIf="reactiveForm6.get('date_ofjoining')?.hasError('required') && reactiveForm6.get('date_ofjoining')?.touched || reactiveForm6.get('date_ofjoining')?.hasError('pattern')"
                            class="text-red">Date of Joining is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Date of Relieving</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="date_ofreleiving" type="date"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': date_ofreleiving.invalid && date_ofreleiving.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="" pattern="^(?!\s*$).+" name="date_ofreleiving" />
                          <div
                            *ngIf="reactiveForm6.get('date_ofreleiving')?.hasError('required') && reactiveForm6.get('date_ofreleiving')?.touched || reactiveForm6.get('date_ofreleiving')?.hasError('pattern')"
                            class="text-red">Date of Relieving is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Working Period</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="work_period" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input"
                            placeholder="Enter Working Period" name="work_period" />
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Reason To Leave Previous
                          Company</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="reason" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input"
                            placeholder="Reason To Leave" name="reason" />
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Department</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="department" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': department.invalid && department.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter Department" pattern="^(?!\s*$).+" name="department" required />
                          <div
                            *ngIf="reactiveForm6.get('department')?.hasError('required') && reactiveForm6.get('department')?.touched || reactiveForm6.get('department')?.hasError('pattern')"
                            class="text-red">Department is required.</div>
                        </div>
                      </div>


                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Reporting To</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="report" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white border-secondary custom-input"
                            placeholder="Enter Manager Name" name="report" />
                        </div>
                      </div>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">HR Name</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input formControlName="HR_name" type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 bg-white custom-input"
                            [ngStyle]="{'border-color': HR_name.invalid && HR_name.touched ? 'red' : 'rgb(209, 209, 209)'}"
                            placeholder="Enter HR Name" pattern="^(?!\s*$).+" name="HR_name" required />
                          <div
                            *ngIf="reactiveForm6.get('HR_name')?.hasError('required') && reactiveForm6.get('HR_name')?.touched || reactiveForm6.get('HR_name')?.hasError('pattern')"
                            class="text-red">HR Name is required.</div>
                        </div>
                      </div>

                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold">Remarks</span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <textarea
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 border-secondary bg-white custom-input"
                            formControlName="rmrks" rows="4" cols="18" placeholder="Enter Remarks"
                            name="rmrks"></textarea>
                        </div>
                      </div>
                    </div>

                    <div class="text-center my-4 mt-10">
                      <button class="btn-success btn-sm text-white me-2" (click)="workexperiencesubmit()"
                        [disabled]="reactiveForm6.invalid">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
                      </button>
                    </div>
                  </div>
                </form>
              </div>

              <!--Nomination-->
              <div class="row tab-pane fade ms-5" id="kt_tab_pane_7" role="tabpanel">
                <form [formGroup]="reactiveForm5" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa-solid fa-award fs-4 me-5"></i>Nomination
                      </h3>
                    </div>

                    <div class="d-flex flex-row flex-wrap mt-6">
                      <div class="col-12 col-lg-4 col-md-4 col-sm-12"><span class="font-bold required">Name</span>
