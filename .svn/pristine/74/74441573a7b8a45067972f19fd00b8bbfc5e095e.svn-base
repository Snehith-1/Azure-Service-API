<div class="card m-3">
    <!-- begin::Heading -->
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">E-Invoice</h5>
        </div>
    </div>
    <!-- end::Heading -->
    <div class="card m-3">
        <form [formGroup]="einvoicedtlform" (ngSubmit)="submit()">
            <div class="card_heading">
                <h6>Transaction Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Tax Scheme<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="transactionDetails_TaxSch" [ngStyle]="{'border-color': transdlttaxschemeControl.invalid && transdlttaxschemeControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="transdlttaxschemeControl.invalid && (transdlttaxschemeControl.dirty || transdlttaxschemeControl.touched)">
                                <div *ngIf="transdlttaxschemeControl.invalid && transdlttaxschemeControl.touched" class="invalid-feedback">
                                    <div *ngIf="transdlttaxschemeControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Tax scheme is required</div>
                                </div>
                            </div>
                            <input formControlName="invoice_gid" hidden placeholder="Address Line 1" type="text" class="form-control form-control-lg form-control-solid bg-white custom-input mb-3 mb-lg-0 borderc-secondary" name="invoice_gid" />
                            <input formControlName="invoicerefno" hidden placeholder="Address Line 1" type="text" class="form-control form-control-lg form-control-solid bg-white custom-input mb-3 mb-lg-0 borderc-secondary" name="invoicerefno" />
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">IGST on Intra Charges</label>
                            </div>
                            <div class="col-lg-3">
                                <select class="form-select form-select-solid bg-white form-select-lg custom-input select2 options" formControlName="transactionDetails_IgstOnIntra">
                                    <option value="">Select Charges</option>
                                    <option value="Y">Yes</option>
                                    <option value="N">No</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Transaction Category<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="transactionDetails_SupTyp" [ngStyle]="{'border-color': transdlttranscatControl.invalid && transdlttranscatControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="transdlttranscatControl.invalid && (transdlttranscatControl.dirty || transdlttranscatControl.touched)">
                                <div *ngIf="transdlttranscatControl.invalid && transdlttranscatControl.touched" class="invalid-feedback">
                                    <div *ngIf="transdlttranscatControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Transaction category is required</div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Reverse Charge</label>
                        </div>

                        <div class="col-lg-3">
                            <select class="form-select form-select-solid bg-white form-select-lg custom-input select2 options" formControlName="transactionDetails_RegRev">
                                <option value="">Select Charges</option>
                                <option value="Y">Yes</option>
                                <option value="N">No</option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Transaction Type</label>
                        </div>
                        <div class="col-lg-3">
                            <select class="form-select form-select-solid bg-white form-select-lg custom-input select2 options" formControlName="transactionDetails_tranaction_type">
                                <option value="">Select Type</option>
                                <option value="Regular">Regular</option>
                                <option value="Ship To">Ship To</option>
                                <option value="Ship From">Ship From</option>
                                <option value="Both 2 and 3">Both 2 and 3</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Document Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Document Type</label>
                            </div>
                            <div class="col-lg-3">
                                <select name="" id="" class="form-select form-select-solid bg-white form-select-lg custom-input select2 options" formControlName="documentDetails_Typ">
                                    <option value="">Select Type</option>
                                    <option value="INV">Invoice</option>
                                    <option value="Credit Note">Credit Note</option>
                                    <option value="Debit Note">Debit Note</option>
                                </select>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Invoice Date</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="invoice_date" readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Invoice No</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="invoicerefno" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Payment Term (In Days)</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="documentDetails_payment_term" readonly>
                        </div>
                        <div class="col-lg-3">
                            <label style="margin-top: 14px; margin-left: 105px; font-size: 12px; font-weight: bold; width: 80px;">Due Date</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input" style="margin-left: 45px; font-size: 12px; width: 85px; border-radius: 0px;" formControlName="documentDetails_due_date" readonly>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Buyer Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Customer GSTIN</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_Gstin" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Company Name</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_LglNm" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Contact Number</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_cont_num" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Customer Address</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <textarea type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input1" formControlName="buyerDetails_Address" rows="1" readonly></textarea>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Pincode</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_Pin" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Remarks</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_remarks" readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Buyer POS</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_Pos" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Contact Person</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_cont_person" readonly>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Email Address</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_email" readonly>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Location</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_Loc" readonly>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">State</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="buyerDetails_Stcd" readonly>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Seller Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Seller GSTIN<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="sellerDetails_Gstin" [ngStyle]="{'border-color': sellerDetails_sellergstinControl.invalid && sellerDetails_sellergstinControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="sellerDetails_sellergstinControl.invalid && (sellerDetails_sellergstinControl.dirty || sellerDetails_sellergstinControl.touched)">
                                <div *ngIf="sellerDetails_sellergstinControl.invalid && sellerDetails_sellergstinControl.touched" class="invalid-feedback">
                                    <div *ngIf="sellerDetails_sellergstinControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Seller gst is required</div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Trade Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="sellerDetails_TrdNm" [ngStyle]="{'border-color': sellerDetails_tradenameControl.invalid && sellerDetails_tradenameControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="sellerDetails_tradenameControl.invalid && (sellerDetails_tradenameControl.dirty || sellerDetails_tradenameControl.touched)" readonly>
                                <div *ngIf="sellerDetails_tradenameControl.invalid && sellerDetails_tradenameControl.touched" class="invalid-feedback">
                                    <div *ngIf="sellerDetails_tradenameControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Seller trade name is required</div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Location</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="sellerDetails_Loc" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">State</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="sellerDetails_Stcd" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Email Address</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="sellerDetails_email" readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Legal Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="sellerDetails_LglNm" [ngStyle]="{'border-color': sellerDetails_legalnameControl.invalid && sellerDetails_legalnameControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="sellerDetails_legalnameControl.invalid && (sellerDetails_legalnameControl.dirty || sellerDetails_legalnameControl.touched)" readonly>
                                <div *ngIf="sellerDetails_legalnameControl.invalid && sellerDetails_legalnameControl.touched" class="invalid-feedback">
                                    <div *ngIf="sellerDetails_legalnameControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Seller legal name is required</div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Address</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <textarea type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input1" formControlName="sellerDetails_Address" rows="1" readonly></textarea>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Pincode<span style="color: red"> *</span></label>
                        </div>
                        <div class="col-lg-3">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="sellerDetails_Pin" [ngStyle]="{'border-color': sellerDetails_pincodeControl.invalid && sellerDetails_pincodeControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="sellerDetails_pincodeControl.invalid && (sellerDetails_pincodeControl.dirty || sellerDetails_pincodeControl.touched)">
                            <div *ngIf="sellerDetails_pincodeControl.invalid && sellerDetails_pincodeControl.touched" class="invalid-feedback">
                                <div *ngIf="sellerDetails_pincodeControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Seller pincode is required</div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Contact Number</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-secondary custom-input left_input" formControlName="sellerDetails_cont_num" readonly>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Dispatch Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="dispatchDetails_Nm" [ngStyle]="{'border-color': dispatchDetails_nameControl.invalid && dispatchDetails_nameControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="dispatchDetails_nameControl.invalid && (dispatchDetails_nameControl.dirty || dispatchDetails_nameControl.touched)">
                                <div *ngIf="dispatchDetails_nameControl.invalid && dispatchDetails_nameControl.touched" class="invalid-feedback">
                                    <div *ngIf="dispatchDetails_nameControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Name is required</div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Location<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="dispatchDetails_Loc" [ngStyle]="{'border-color': dispatchDetails_locationControl.invalid && dispatchDetails_locationControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="dispatchDetails_locationControl.invalid && (dispatchDetails_locationControl.dirty || dispatchDetails_locationControl.touched)">
                                <div *ngIf="dispatchDetails_locationControl.invalid && dispatchDetails_locationControl.touched" class="invalid-feedback">
                                    <div *ngIf="dispatchDetails_locationControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Dispatch location is required</div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">State<span style="color: red"> *</span></label>
                            </div>

                            <div class="col-lg-3">
                                <input type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input" formControlName="dispatchDetails_Stcd" [ngStyle]="{'border-color': dispatchDetails_stateControl.invalid && dispatchDetails_stateControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="dispatchDetails_stateControl.invalid && (dispatchDetails_stateControl.dirty || dispatchDetails_stateControl.touched)">
                                <div *ngIf="dispatchDetails_stateControl.invalid && dispatchDetails_stateControl.touched" class="invalid-feedback">
                                    <div *ngIf="dispatchDetails_stateControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Dispatch state is required</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Address<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <textarea type="text" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0 bg-white custom-input left_input1" formControlName="dispatchDetails_Address" rows="1" [ngStyle]="{'border-color': dispatchDetails_addressControl.invalid && dispatchDetails_addressControl.touched ? 'red' : 'rgb(209, 209, 209)'}" pattern="^(?!\s*$).+" [class.is-invalid]="dispatchDetails_addressControl.invalid && (dispatchDetails_addressControl.dirty || dispatchDetails_addressControl.touched)"></textarea>
                                <div *ngIf="dispatchDetails_addressControl.invalid && dispatchDetails_addressControl.touched" class="invalid-feedback">
                                    <div *ngIf="dispatchDetails_addressControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Dispatch address is required</div>
                                </div>
