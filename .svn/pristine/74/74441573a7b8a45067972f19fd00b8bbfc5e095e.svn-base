                                <div *ngIf="shipDetails_tradenameshipControl.invalid && shipDetails_tradenameshipControl.touched" class="invalid-feedback">
                                    <div *ngIf="shipDetails_tradenameshipControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Shipping trade name is required</div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Location<span style="color: red"> *</span></label>
                            </div>
