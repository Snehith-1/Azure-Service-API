            <!-- begin::table -->
            <div class="table-responsive">
                <div class="card card-body m-0 shadow-lg p-3 mb-5 bg-white rounded">
                    <table id="product" class="table table-striped table-row-bordered gy-4 gs-2">
                        <thead class="table_head">
                            <tr class="text-white fw-bold fs-5 text-nowrap text-blue">
                                <th>S.No</th>
                                <th>Product Name</th>
                                <th>Product Group</th>
                                <th>Product Code</th>
                                <th>HSN Code / Description</th>
                                <th>Unit</th>
                                <th style="text-align: right;">Unit Price</th>
                                <th style="text-align: right;">MRP</th>
                                <th style="text-align: center;">Quantity</th>
                                <th style="text-align: right;">Discount (%)</th>
                                <th style="text-align: center; width: 120px;">Tax</th>
                                <th style="text-align: right;">Total Amount</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr *ngFor="let data of invoiceproductlist; let i=index;" class="align-middle">
                                <td>{{i+1}}</td>
                                <td>{{data.product_name}}</td>
                                <td>{{data.productgroup_name}}</td>
                                <td>{{data.product_code}}</td>
                                <td>{{data.hsn}}</td>
                                <td>{{data.uom_name}}</td>
                                <td style="text-align: right;">{{data.product_price }}</td>
                                <td style="text-align: right;">{{data.selling_price }}</td>
                                <td style="text-align: center;">{{data.qty_invoice}}</td>
                                <td style="text-align: right;">{{data.discount}}</td>
                                <td style="text-align: right;">{{data.tax}}</td>
                                <td class="fw-bold" style="text-align: right;">{{data.product_total }}</td>
                                <td class="text-center text-nowrap">
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <!-- end::table -->

            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Net Amount :</label>
                    <input type="text" class="in fw-bold" placeholder="0.00" formControlName="netamount" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Addon Amount :</label>
                    <input type="text" class="in" placeholder="0.00" formControlName="addoncharges" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Additional Discount :</label>
                    <input type="text" class="in" placeholder="0.00" formControlName="invoicediscountamount" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Freight Charges :</label>
                    <input type="text" class="in" placeholder="0.00" formControlName="frieghtcharges" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <!-- <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Buyback Charges :</label>
                    <input type="text" class="in" placeholder="0.00" formControlName="buybackcharges">
                    <span class="spn">INR</span>
                </div>
            </div> -->
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Packing/Forwarding Charges :</label>
                    <input type="text" class="in"  placeholder="0.00" formControlName="packing_charges" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Insurance Charges :</label>
                    <input type="text" class="in" placeholder="0.00" formControlName="insurancecharges" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Round Off :</label>
                    <input type="text" class="in" placeholder="0.00" formControlName="roundoff" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-4 amt_in">
                    <label class="lab_name">Grand Total :</label>
                    <input type="text" class="in fw-bold"  placeholder="0.00" formControlName="invoice_amount" readonly>
                    <span class="spn">INR</span>
                </div>
            </div>
        </form>
    </div>

    <div class="card" m-3>
        <div class="text-center my-4">
            <button type="submit" class="btn-success btn-sm text-white me-4" (click)="submit()">
                <i class="bi bi-check-square fs-7 me-1"></i>
                Submit
            </button>

            <button class="btn-primary btn-sm text-white me-4" (click)="back()">
                <i class="bi bi-arrow-left-square fs-7 me-1"></i>
                Back
            </button>
        </div>
    </div>
</div>