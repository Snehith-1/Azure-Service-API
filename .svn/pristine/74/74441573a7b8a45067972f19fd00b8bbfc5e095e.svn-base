                                <div *ngIf="shipDetails_gstinControl.invalid && shipDetails_gstinControl.touched" class="invalid-feedback">
                                    <div *ngIf="shipDetails_gstinControl.hasError('required')" style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">* Shipping gst is required</div>
                                </div>
                            </div>
                        </div>

