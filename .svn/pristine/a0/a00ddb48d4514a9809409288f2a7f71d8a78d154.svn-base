<div class="card">
    <div class="row ms-3 mt-3 d-flex align-items-center">
        <div class="col">
          <h3 class="card-title">Delivery Acknowledgement Summary</h3>
        </div>
        <div class="col me-5">
          <button type="button" class="btn-primary btn-sm text-white me-4 float-end" data-bs-toggle="modal"
            data-bs-target="#myModaladd" routerLink="/ims/ImsTrnDeliveryAcknowledgemantadd"><i class="fas fa-plus"></i>&nbsp;&nbsp;
            Raise DO Acknowledgement
          </button>
        </div>
      </div> 
    <div class="table-responsive">
        <div class="mx-3 p-3 pt-0 pb-0 bg-white rounded">
          <table id="deliverysummary_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
            <thead>
              <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                <th>S.NO </th>
                <th>DO Date</th>
                <th>DO Reference .No</th>
                <th>Customer  </th>
                <th>Contact Details </th>
                <th>Status </th>
                <th>Delivery Details</th>
                <th>Delivery By</th>
                <th>Delivery Remarks</th>
              </tr>
            </thead>
            <tbody>
              <tr *ngFor="let data of deliverysummary_list; let i=index;">
                <td>{{i+1}}</td>
                <td>{{data.directorder_date}}</td>
                <td>{{data.directorder_refno}}</td> 
                <td>{{data.customer_name}}</td>
                <td>{{data.customer_contactperson}}</td>  
                <td>{{data.delivery_status}}</td>       
                <td>{{data.delivery_details}}</td>
                <td>{{data.delivered_by}}</td>
                <td>{{data.delivered_remarks}}</td>
    
           
                
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    
